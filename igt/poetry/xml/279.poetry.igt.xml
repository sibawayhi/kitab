   <verses id="٤٩٥٨٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كَذَّبْتْك عَيْنُكَ أَمْ رَأَيْتَ بِوَاْسْطٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَذَّبْتْك</ar>
               </t>
               <t>
                  <ar>عَيْنُكَ</ar>
               </t>
               <t>
                  <ar>أَمْ</ar>
               </t>
               <t>
                  <ar>رَأَيْتَ</ar>
               </t>
               <t>
                  <ar>بِوَاْسْطٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>غَلَسَ الظّلَاْمِ مِنْ الرَّبَاْبِ خَيَاْلَاَْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>غَلَسَ</ar>
               </t>
               <t>
                  <ar>الظّلَاْمِ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>الرَّبَاْبِ</ar>
               </t>
               <t>
                  <ar>خَيَاْلَاَْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٠٥٤٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَلَيْسَ أَبِي بِاْلنَّضْرِ أَمْ لَيْسَ وَالِدِي</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَلَيْسَ</ar>
               </t>
               <t>
                  <ar>أَبِي</ar>
               </t>
               <t>
                  <ar>بِاْلنَّضْرِ</ar>
               </t>
               <t>
                  <ar>أَمْ</ar>
               </t>
               <t>
                  <ar>لَيْسَ</ar>
               </t>
               <t>
                  <ar>وَالِدِي</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>لَكُلْ نَجِيْبٍ مِنْ خُزَاْعَةَ أَزْهَرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَكُلْ</ar>
               </t>
               <t>
                  <ar>نَجِيْبٍ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>خُزَاْعَةَ</ar>
               </t>
               <t>
                  <ar>أَزْهَرَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٢١٣١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَعَمْرُك مَا أَدْرِي وَإِنْ كُنْتُ دَاْرِيَاًْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَعَمْرُك</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>أَدْرِي</ar>
               </t>
               <t>
                  <ar>وَإِنْ</ar>
               </t>
               <t>
                  <ar>كُنْتُ</ar>
               </t>
               <t>
                  <ar>دَاْرِيَاًْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>شُعَيْثُ بْنُ سَهْمٍ أُمُّ شُعَيْثُ بْنِ مِنْقَرِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>شُعَيْثُ</ar>
               </t>
               <t>
                  <ar>بْنُ</ar>
               </t>
               <t>
                  <ar>سَهْمٍ</ar>
               </t>
               <t>
                  <ar>أُمُّ</ar>
               </t>
               <t>
                  <ar>شُعَيْثُ</ar>
               </t>
               <t>
                  <ar>بْنِ</ar>
               </t>
               <t>
                  <ar>مِنْقَرِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
