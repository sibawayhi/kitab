<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="066">
   <verses id="٤٨٧٥٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>عَمَّرْتُكِ الله إِلَّا مَا ذَكَرْتِ لَنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>عَمَّرْتُكِ</ar>
               </t>
               <t>
                  <ar>الله</ar>
               </t>
               <t>
                  <ar>إِلَّا</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>ذَكَرْتِ</ar>
               </t>
               <t>
                  <ar>لَنَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>هَلْ كُنْتِ جَاْرتَنَا أَيَّاْمَ ذِي سَلَمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>هَلْ</ar>
               </t>
               <t>
                  <ar>كُنْتِ</ar>
               </t>
               <t>
                  <ar>جَاْرتَنَا</ar>
               </t>
               <t>
                  <ar>أَيَّاْمَ</ar>
               </t>
               <t>
                  <ar>ذِي</ar>
               </t>
               <t>
                  <ar>سَلَمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٧٧٤٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>عَمَّرتُكَ اللهَ الْجَلِيْلَ فَإِنَّنِي</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>عَمَّرتُكَ</ar>
               </t>
               <t>
                  <ar>اللهَ</ar>
               </t>
               <t>
                  <ar>الْجَلِيْلَ</ar>
               </t>
               <t>
                  <ar>فَإِنَّنِي</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَلْوِي عَلَيْكَ لَوْ أَنَّ لُبَّكَ يَهْتَدِى</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَلْوِي</ar>
               </t>
               <t>
                  <ar>عَلَيْكَ</ar>
               </t>
               <t>
                  <ar>لَوْ</ar>
               </t>
               <t>
                  <ar>أَنَّ</ar>
               </t>
               <t>
                  <ar>لُبَّكَ</ar>
               </t>
               <t>
                  <ar>يَهْتَدِى</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٩٦٤٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَقَوْلُ لما جَاْءَنِي فَخْرُهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَقَوْلُ</ar>
               </t>
               <t>
                  <ar>لما</ar>
               </t>
               <t>
                  <ar>جَاْءَنِي</ar>
               </t>
               <t>
                  <ar>فَخْرُهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>سُبْحَاْنَ مِنْ عَلْقَمَةَ الفَاْخِرِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>سُبْحَاْنَ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>عَلْقَمَةَ</ar>
               </t>
               <t>
                  <ar>الفَاْخِرِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٣٨٥٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>سَلَاْمَكَ رَبَّنَا فِي كُلِّ فَجْرٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>سَلَاْمَكَ</ar>
               </t>
               <t>
                  <ar>رَبَّنَا</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>كُلِّ</ar>
               </t>
               <t>
                  <ar>فَجْرٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>بَرِيْئًا مَا تَغَنَّثُكَ الذُّمُوْمُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بَرِيْئًا</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>تَغَنَّثُكَ</ar>
               </t>
               <t>
                  <ar>الذُّمُوْمُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٩٥٤٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>سُبحَاْنَهُ ثُمَّ سُبْحَاْنًا يَعَوْدُ لَهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>سُبحَاْنَهُ</ar>
               </t>
               <t>
                  <ar>ثُمَّ</ar>
               </t>
               <t>
                  <ar>سُبْحَاْنًا</ar>
               </t>
               <t>
                  <ar>يَعَوْدُ</ar>
               </t>
               <t>
                  <ar>لَهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَقَبْلَنَا سَبَّحَ الجُوْدِىُّ وَالجُمُدُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَقَبْلَنَا</ar>
               </t>
               <t>
                  <ar>سَبَّحَ</ar>
               </t>
               <t>
                  <ar>الجُوْدِىُّ</ar>
               </t>
               <t>
                  <ar>وَالجُمُدُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
