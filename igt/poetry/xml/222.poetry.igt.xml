<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="222">
   <verses id="٨٧٦٩٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَلَقَدْ أَبِيْتُ مِنْ الْفَتَاْةِ بِمَنْزِلٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَلَقَدْ</ar>
               </t>
               <t>
                  <ar>أَبِيْتُ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>الْفَتَاْةِ</ar>
               </t>
               <t>
                  <ar>بِمَنْزِلٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَأَبِيْتُ لَا حَرَجٌ وَلَا مَحْرُوْمُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَأَبِيْتُ</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>حَرَجٌ</ar>
               </t>
               <t>
                  <ar>وَلَا</ar>
               </t>
               <t>
                  <ar>مَحْرُوْمُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٠٢٦٧٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَأَّيِّي مَا وَأَيُّكَ كَاْنَ شَرًّا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَأَّيِّي</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>وَأَيُّكَ</ar>
               </t>
               <t>
                  <ar>كَاْنَ</ar>
               </t>
               <t>
                  <ar>شَرًّا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَسِيْقَ إَلى الْمُقَاْمَةِ لا يَرَاْهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَسِيْقَ</ar>
               </t>
               <t>
                  <ar>إَلى</ar>
               </t>
               <t>
                  <ar>الْمُقَاْمَةِ</ar>
               </t>
               <t>
                  <ar>لا</ar>
               </t>
               <t>
                  <ar>يَرَاْهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٦٢٤٣٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَلَقَدْ عَلِمْتُ إِذَا الرِّجَاْلُ تَنَاْهَزُوْا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَلَقَدْ</ar>
               </t>
               <t>
                  <ar>عَلِمْتُ</ar>
               </t>
               <t>
                  <ar>إِذَا</ar>
               </t>
               <t>
                  <ar>الرِّجَاْلُ</ar>
               </t>
               <t>
                  <ar>تَنَاْهَزُوْا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَيِّي وَأَيُّكُمُ أَعَزُّ وَأَمْنَعُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَيِّي</ar>
               </t>
               <t>
                  <ar>وَأَيُّكُمُ</ar>
               </t>
               <t>
                  <ar>أَعَزُّ</ar>
               </t>
               <t>
                  <ar>وَأَمْنَعُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٦٢٢١٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَأَيِّي وَأَىُّ ابْنِ الْحُصَيِنِ وَعَثْعَثٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَأَيِّي</ar>
               </t>
               <t>
                  <ar>وَأَىُّ</ar>
               </t>
               <t>
                  <ar>ابْنِ</ar>
               </t>
               <t>
                  <ar>الْحُصَيِنِ</ar>
               </t>
               <t>
                  <ar>وَعَثْعَثٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>غَدَاْةَ الْتَقَيْنَا كَاْنَ عِنْدَكَ أَعْذَرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>غَدَاْةَ</ar>
               </t>
               <t>
                  <ar>الْتَقَيْنَا</ar>
               </t>
               <t>
                  <ar>كَاْنَ</ar>
               </t>
               <t>
                  <ar>عِنْدَكَ</ar>
               </t>
               <t>
                  <ar>أَعْذَرَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
