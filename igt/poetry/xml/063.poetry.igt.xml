<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="063">
   <verses id="٧١٢٧٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أِلَى إمَاْمٍ تُغَاْدِيْنا فَوَاْضِلُه</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أِلَى</ar>
               </t>
               <t>
                  <ar>إمَاْمٍ</ar>
               </t>
               <t>
                  <ar>تُغَاْدِيْنا</ar>
               </t>
               <t>
                  <ar>فَوَاْضِلُه</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَظْفَرَهَ اللهُ فَلْيَهْنِئْ لَهُ الظَّفَرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَظْفَرَهَ</ar>
               </t>
               <t>
                  <ar>اللهُ</ar>
               </t>
               <t>
                  <ar>فَلْيَهْنِئْ</ar>
               </t>
               <t>
                  <ar>لَهُ</ar>
               </t>
               <t>
                  <ar>الظَّفَرُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩٣٥٠٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>هَنِيْئًا لِأَربَاْبِ الْبُيُوْتِ بُيُوْتِهِمْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>هَنِيْئًا</ar>
               </t>
               <t>
                  <ar>لِأَربَاْبِ</ar>
               </t>
               <t>
                  <ar>الْبُيُوْتِ</ar>
               </t>
               <t>
                  <ar>بُيُوْتِهِمْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَلِلْعَزَبِ الْمِسْكِيْنِ مَا يَتَلَمَّسُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَلِلْعَزَبِ</ar>
               </t>
               <t>
                  <ar>الْمِسْكِيْنِ</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>يَتَلَمَّسُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
