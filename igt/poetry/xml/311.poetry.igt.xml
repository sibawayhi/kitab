   <verses id="٩٤٧١٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَرِيْشِي مِنْكُمُ وَهَوَاْيَ مَعْكُمْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَرِيْشِي</ar>
               </t>
               <t>
                  <ar>مِنْكُمُ</ar>
               </t>
               <t>
                  <ar>وَهَوَاْيَ</ar>
               </t>
               <t>
                  <ar>مَعْكُمْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَإِنْ كَاْنَتْ زِيَاْرَتُكُمْ لَمِاْمَاْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَإِنْ</ar>
               </t>
               <t>
                  <ar>كَاْنَتْ</ar>
               </t>
               <t>
                  <ar>زِيَاْرَتُكُمْ</ar>
               </t>
               <t>
                  <ar>لَمِاْمَاْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٦٢٧٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يَا لَيْتَهَا كَاْنَتْ لِأَهْلِي إِبِلَاْ</ar>
               <free/>
            </text>
            <igt>
