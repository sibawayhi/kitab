               <t>
                  <ar>هُزِلَتْ</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>جَدْبِ</ar>
               </t>
               <t>
                  <ar>عَاْمٍ</ar>
               </t>
               <t>
                  <ar>أَوَّلَاْ</ar>
