<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="174">
   <verses id="٣٦٢٩٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَقَدْ وَسَطْتُ مَاْلِكًا وَحَنْظَلًاْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَقَدْ</ar>
               </t>
               <t>
                  <ar>وَسَطْتُ</ar>
               </t>
               <t>
                  <ar>مَاْلِكًا</ar>
               </t>
               <t>
                  <ar>وَحَنْظَلًاْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩٥٤٥٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَبُو حَنَشٍ يُؤَرِّقُنَا وَطَلْقٌ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَبُو</ar>
               </t>
               <t>
                  <ar>حَنَشٍ</ar>
               </t>
               <t>
                  <ar>يُؤَرِّقُنَا</ar>
               </t>
               <t>
                  <ar>وَطَلْقٌ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَعَمّاْرٌ وَآوِنَةً أُثَاْلَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَعَمّاْرٌ</ar>
               </t>
               <t>
                  <ar>وَآوِنَةً</ar>
               </t>
               <t>
                  <ar>أُثَاْلَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٦٥٥٦٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَلَا أَضْحَّتْ حِبَاْلُكُمْ رِمَاْمَاًْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَلَا</ar>
               </t>
               <t>
                  <ar>أَضْحَّتْ</ar>
               </t>
               <t>
                  <ar>حِبَاْلُكُمْ</ar>
               </t>
               <t>
                  <ar>رِمَاْمَاًْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَأَضَحْتْ مِنْكَ شَاْسِعَةً أُمَاْمَاَْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَأَضَحْتْ</ar>
               </t>
               <t>
                  <ar>مِنْكَ</ar>
               </t>
               <t>
                  <ar>شَاْسِعَةً</ar>
               </t>
               <t>
                  <ar>أُمَاْمَاَْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>يَشُقُّ بِهَا الْعَسَاْقِلَ مُؤْجِّدَاْتٌ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَشُقُّ</ar>
               </t>
               <t>
                  <ar>بِهَا</ar>
               </t>
               <t>
                  <ar>الْعَسَاْقِلَ</ar>
               </t>
               <t>
                  <ar>مُؤْجِّدَاْتٌ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَكُلُّ عَرَنْدَسٍ يَنْفِى الُّلَّغَاْمَاَْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَكُلُّ</ar>
               </t>
               <t>
                  <ar>عَرَنْدَسٍ</ar>
               </t>
               <t>
                  <ar>يَنْفِى</ar>
               </t>
               <t>
                  <ar>الُّلَّغَاْمَاَْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٧١٤٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>خُذُوْا حَظَّكُمْ يَا آلَ عِكْرِمَ وَاذْكُرُوَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>خُذُوْا</ar>
               </t>
               <t>
                  <ar>حَظَّكُمْ</ar>
               </t>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>آلَ</ar>
               </t>
               <t>
                  <ar>عِكْرِمَ</ar>
               </t>
               <t>
                  <ar>وَاذْكُرُوَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَوَاْصِرَنَا وَالرِّحْمُ بِاْلْغَيْبِ تُذْكَرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَوَاْصِرَنَا</ar>
               </t>
               <t>
                  <ar>وَالرِّحْمُ</ar>
               </t>
               <t>
                  <ar>بِاْلْغَيْبِ</ar>
               </t>
               <t>
                  <ar>تُذْكَرُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٢٦٧٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>إِنّ ابْنَ حَاْرِثَ إنْ أَشْتَقْ لَرُؤْيِتُهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِنّ</ar>
               </t>
               <t>
                  <ar>ابْنَ</ar>
               </t>
               <t>
                  <ar>حَاْرِثَ</ar>
               </t>
               <t>
                  <ar>إنْ</ar>
               </t>
               <t>
                  <ar>أَشْتَقْ</ar>
               </t>
               <t>
                  <ar>لَرُؤْيِتُهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أُو أَمْتَدِحْهُ فَإِنّ النَّاْسَ قَدْ عَلِمُوَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أُو</ar>
               </t>
               <t>
                  <ar>أَمْتَدِحْهُ</ar>
               </t>
               <t>
                  <ar>فَإِنّ</ar>
               </t>
               <t>
                  <ar>النَّاْسَ</ar>
               </t>
               <t>
                  <ar>قَدْ</ar>
               </t>
               <t>
                  <ar>عَلِمُوَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٠٤٢٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَوْدَى ابْنُ جُلْهُمَ عَبّاْدٌ بِصَرْمِتِهِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَوْدَى</ar>
               </t>
               <t>
                  <ar>ابْنُ</ar>
               </t>
               <t>
                  <ar>جُلْهُمَ</ar>
               </t>
               <t>
                  <ar>عَبّاْدٌ</ar>
               </t>
               <t>
                  <ar>بِصَرْمِتِهِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>إِنّ ابْنُ جُلْهُمَ أَمْسَى حَيّةَ الْوَاْدِى</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِنّ</ar>
               </t>
               <t>
                  <ar>ابْنُ</ar>
               </t>
               <t>
                  <ar>جُلْهُمَ</ar>
               </t>
               <t>
                  <ar>أَمْسَى</ar>
               </t>
               <t>
                  <ar>حَيّةَ</ar>
               </t>
               <t>
                  <ar>الْوَاْدِى</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٥٢٢١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَهَا أَشَاْرِيْرُ مِنْ لْحَمٍ تُتَمّرُهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَهَا</ar>
               </t>
               <t>
                  <ar>أَشَاْرِيْرُ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>لْحَمٍ</ar>
               </t>
               <t>
                  <ar>تُتَمّرُهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>مُنَ الثَّعَاْلِى وَوَخْزٌ مِنْ ؟ ؟</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مُنَ</ar>
               </t>
               <t>
                  <ar>الثَّعَاْلِى</ar>
               </t>
               <t>
                  <ar>وَوَخْزٌ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>؟</ar>
               </t>
               <t>
                  <ar>؟</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٦٤٩٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَمَنْهَلٍ لَيْسَ لَهُ حَوَاْزِقُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَمَنْهَلٍ</ar>
               </t>
               <t>
                  <ar>لَيْسَ</ar>
               </t>
               <t>
                  <ar>لَهُ</ar>
               </t>
               <t>
                  <ar>حَوَاْزِقُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>ولِضَفَاْدِى جَمِّهِ نَقَاْنِقُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ولِضَفَاْدِى</ar>
               </t>
               <t>
                  <ar>جَمِّهِ</ar>
