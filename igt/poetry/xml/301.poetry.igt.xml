<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="301">
   <verses id="٤٧٣٥٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَهَا زَجَلٌ كَحَفِيْفِ الْحَصَاْدِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَهَا</ar>
               </t>
               <t>
                  <ar>زَجَلٌ</ar>
               </t>
               <t>
                  <ar>كَحَفِيْفِ</ar>
               </t>
               <t>
                  <ar>الْحَصَاْدِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>صَاْدَفَ بِاْللَّيْلِ رِيْحًا دَبُوْرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>صَاْدَفَ</ar>
               </t>
               <t>
                  <ar>بِاْللَّيْلِ</ar>
               </t>
               <t>
                  <ar>رِيْحًا</ar>
               </t>
               <t>
                  <ar>دَبُوْرَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٤١٥١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>حَاْلَتْ وَحِيْلَ بِهَا وَغَيَّرَ آيَهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>حَاْلَتْ</ar>
               </t>
               <t>
                  <ar>وَحِيْلَ</ar>
               </t>
               <t>
                  <ar>بِهَا</ar>
               </t>
               <t>
                  <ar>وَغَيَّرَ</ar>
               </t>
               <t>
                  <ar>آيَهَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>صَرْفُ الْبِلَى تَجْرِي بِهِ الرِيّحَاْنِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>صَرْفُ</ar>
               </t>
               <t>
                  <ar>الْبِلَى</ar>
               </t>
               <t>
                  <ar>تَجْرِي</ar>
               </t>
               <t>
                  <ar>بِهِ</ar>
               </t>
               <t>
                  <ar>الرِيّحَاْنِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>رِيْحُ الْجَنُوْبِ مَعَ الشَّمَاْلِ وَتَاْرَةً</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>رِيْحُ</ar>
               </t>
               <t>
                  <ar>الْجَنُوْبِ</ar>
               </t>
               <t>
                  <ar>مَعَ</ar>
               </t>
               <t>
                  <ar>الشَّمَاْلِ</ar>
               </t>
               <t>
                  <ar>وَتَاْرَةً</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>رِهَمُ الرَّبِيْعِ وَصَاْئِبُ التَّهْتَاْنِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>رِهَمُ</ar>
               </t>
               <t>
                  <ar>الرَّبِيْعِ</ar>
               </t>
               <t>
                  <ar>وَصَاْئِبُ</ar>
               </t>
               <t>
                  <ar>التَّهْتَاْنِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
