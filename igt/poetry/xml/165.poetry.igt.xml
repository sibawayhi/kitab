<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="165">
   <verses id="٢٦٨٥٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يَدْعُوْنَ عَنْتَرُ وَالرِمّاْحُ كَأَنَّهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَدْعُوْنَ</ar>
               </t>
               <t>
                  <ar>عَنْتَرُ</ar>
               </t>
               <t>
                  <ar>وَالرِمّاْحُ</ar>
               </t>
               <t>
                  <ar>كَأَنَّهَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَشْطَاْنٌ بِيْرٍ فِى لَبَاْنِ الْأَدْهَمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَشْطَاْنٌ</ar>
               </t>
               <t>
                  <ar>بِيْرٍ</ar>
               </t>
               <t>
                  <ar>فِى</ar>
               </t>
               <t>
                  <ar>لَبَاْنِ</ar>
               </t>
               <t>
                  <ar>الْأَدْهَمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٦٧١٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَلَا هَلْ لِهٰذَا الدَّهْرِ مِنْ مُتَعَلّلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَلَا</ar>
               </t>
               <t>
                  <ar>هَلْ</ar>
               </t>
               <t>
                  <ar>لِهٰذَا</ar>
               </t>
               <t>
                  <ar>الدَّهْرِ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>مُتَعَلّلِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>عَنَ النَّاْسِ مَهْمَا شَاْءَ بِالنَّاْسِ يَفْعَلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>عَنَ</ar>
               </t>
               <t>
                  <ar>النَّاْسِ</ar>
               </t>
               <t>
                  <ar>مَهْمَا</ar>
               </t>
               <t>
                  <ar>شَاْءَ</ar>
               </t>
               <t>
                  <ar>بِالنَّاْسِ</ar>
               </t>
               <t>
                  <ar>يَفْعَلِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٩٥٠٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَهٰذَا رِدَاْئِى عِنْدَهُ يَسْتَعِيْرُهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَهٰذَا</ar>
               </t>
               <t>
                  <ar>رِدَاْئِى</ar>
               </t>
               <t>
                  <ar>عِنْدَهُ</ar>
               </t>
               <t>
                  <ar>يَسْتَعِيْرُهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>لِيَسْلُبَنِى حَقّي أَمَاْلِ بْنَ حَنْظَلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لِيَسْلُبَنِى</ar>
               </t>
               <t>
                  <ar>حَقّي</ar>
               </t>
               <t>
                  <ar>أَمَاْلِ</ar>
               </t>
               <t>
                  <ar>بْنَ</ar>
               </t>
               <t>
                  <ar>حَنْظَلِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٥٣٤٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>إِمَّا تَرَيْنِي الْيَوْمَ أُمَّ حَمْزِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِمَّا</ar>
               </t>
               <t>
                  <ar>تَرَيْنِي</ar>
               </t>
               <t>
                  <ar>الْيَوْمَ</ar>
               </t>
               <t>
                  <ar>أُمَّ</ar>
               </t>
               <t>
                  <ar>حَمْزِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>قَاْرَبْتُ بَيْنَ عَنَقِي وَجَمْزِي</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>قَاْرَبْتُ</ar>
               </t>
               <t>
                  <ar>بَيْنَ</ar>
               </t>
               <t>
                  <ar>عَنَقِي</ar>
               </t>
               <t>
                  <ar>وَجَمْزِي</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٢٨٧١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>دِيَاْرَ مَيَّةَ إِذْ مَيٌّ تُسَاْعِفُنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>دِيَاْرَ</ar>
               </t>
               <t>
                  <ar>مَيَّةَ</ar>
               </t>
               <t>
                  <ar>إِذْ</ar>
               </t>
               <t>
                  <ar>مَيٌّ</ar>
               </t>
               <t>
                  <ar>تُسَاْعِفُنَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>ولَا يَرَى مِثْلَهَا عُجْمٌ وَلَا عَرَبُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ولَا</ar>
               </t>
               <t>
                  <ar>يَرَى</ar>
               </t>
               <t>
                  <ar>مِثْلَهَا</ar>
               </t>
               <t>
                  <ar>عُجْمٌ</ar>
               </t>
               <t>
                  <ar>وَلَا</ar>
               </t>
               <t>
                  <ar>عَرَبُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٨٤٦٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فِي لَجَّةٍ أَمْسِكْ فُلَاْنًا عَنْ فُلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>لَجَّةٍ</ar>
               </t>
               <t>
                  <ar>أَمْسِكْ</ar>
               </t>
               <t>
                  <ar>فُلَاْنًا</ar>
               </t>
               <t>
                  <ar>عَنْ</ar>
               </t>
               <t>
                  <ar>فُلِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
