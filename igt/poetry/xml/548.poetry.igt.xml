<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="548">
   <verses id="٤٠٢٦٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَقَدْ عَلِمَتْ عِرْسِي مُلَيْكَةُ أَنَّنِي</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَقَدْ</ar>
               </t>
               <t>
                  <ar>عَلِمَتْ</ar>
               </t>
               <t>
                  <ar>عِرْسِي</ar>
               </t>
               <t>
                  <ar>مُلَيْكَةُ</ar>
               </t>
               <t>
                  <ar>أَنَّنِي</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَنَا اللَّيْثُ مَعْدِيًّا عَلَيْهِ وَعَاْدِيَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَنَا</ar>
               </t>
               <t>
                  <ar>اللَّيْثُ</ar>
               </t>
               <t>
                  <ar>مَعْدِيًّا</ar>
               </t>
               <t>
                  <ar>عَلَيْهِ</ar>
               </t>
               <t>
                  <ar>وَعَاْدِيَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
