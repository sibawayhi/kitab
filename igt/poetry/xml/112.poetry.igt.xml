<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="112">
   <verses id="٨٣٤٣٢" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>ظَنَنْتُمْ بِأَنْ يَخْفَى الَّذِي قَدْ صَنَعْتُمُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ظَنَنْتُمْ</ar>
               </t>
               <t>
                  <ar>بِأَنْ</ar>
               </t>
               <t>
                  <ar>يَخْفَى</ar>
               </t>
               <t>
                  <ar>الَّذِي</ar>
               </t>
               <t>
                  <ar>قَدْ</ar>
               </t>
               <t>
                  <ar>صَنَعْتُمُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَفِيْنا نَبيٌّ عِنْدَهُ الْوَحْيُ وَاضِعُهْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَفِيْنا</ar>
               </t>
               <t>
                  <ar>نَبيٌّ</ar>
               </t>
               <t>
                  <ar>عِنْدَهُ</ar>
               </t>
               <t>
                  <ar>الْوَحْيُ</ar>
               </t>
               <t>
                  <ar>وَاضِعُهْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٠٨٠٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَيُّ فَتَى هَيْجَاْءَ أَنْتَ وَجَاْرِهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَيُّ</ar>
               </t>
               <t>
                  <ar>فَتَى</ar>
               </t>
               <t>
                  <ar>هَيْجَاْءَ</ar>
               </t>
               <t>
                  <ar>أَنْتَ</ar>
               </t>
               <t>
                  <ar>وَجَاْرِهَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>إِذَا مَا رِجَاْلٌ بِاْلرِّجَاْلِ استَقَّلَتِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِذَا</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>رِجَاْلٌ</ar>
               </t>
               <t>
                  <ar>بِاْلرِّجَاْلِ</ar>
               </t>
               <t>
                  <ar>استَقَّلَتِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٦٦٢٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَكَمْ دُوْنَ بَيْتِكَ مِنْ صَفْصَفٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَكَمْ</ar>
               </t>
               <t>
                  <ar>دُوْنَ</ar>
               </t>
               <t>
                  <ar>بَيْتِكَ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>صَفْصَفٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَدَكْدَاْكِ رَمْلٍ وَأَعْقَاْدِهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَدَكْدَاْكِ</ar>
               </t>
               <t>
                  <ar>رَمْلٍ</ar>
               </t>
               <t>
                  <ar>وَأَعْقَاْدِهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>وَوَضْعٍ سِقَاْءٍ وَإحْقَاْبِهِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَوَضْعٍ</ar>
               </t>
               <t>
                  <ar>سِقَاْءٍ</ar>
               </t>
               <t>
                  <ar>وَإحْقَاْبِهِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَحَلِّ حُلُوْسٍ وَإغْمَاْدِهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَحَلِّ</ar>
               </t>
               <t>
                  <ar>حُلُوْسٍ</ar>
               </t>
               <t>
                  <ar>وَإغْمَاْدِهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
