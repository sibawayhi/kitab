<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="146">
   <verses id="١٠٠١٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَزَيْدُ أَخَا وَرْقَاْءَ إِنْ كُنْت ثَاْئِرًا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَزَيْدُ</ar>
               </t>
               <t>
                  <ar>أَخَا</ar>
               </t>
               <t>
                  <ar>وَرْقَاْءَ</ar>
               </t>
               <t>
                  <ar>إِنْ</ar>
               </t>
               <t>
                  <ar>كُنْت</ar>
               </t>
               <t>
                  <ar>ثَاْئِرًا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَقَدْ عَرَضَتْ أَحنَاْءُ حَقٍّ فَخَاْصِمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَقَدْ</ar>
               </t>
               <t>
                  <ar>عَرَضَتْ</ar>
               </t>
               <t>
                  <ar>أَحنَاْءُ</ar>
               </t>
               <t>
                  <ar>حَقٍّ</ar>
               </t>
               <t>
                  <ar>فَخَاْصِمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٩٤٠٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>إِنِّي وَأَسْطَاْرٍ سُطِرْنَ سَطْرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِنِّي</ar>
               </t>
               <t>
                  <ar>وَأَسْطَاْرٍ</ar>
               </t>
               <t>
                  <ar>سُطِرْنَ</ar>
               </t>
               <t>
                  <ar>سَطْرَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>لَقاْئِلٌ يَا نَصْرُ نَصْرًا نَصْرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَقاْئِلٌ</ar>
               </t>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>نَصْرُ</ar>
               </t>
               <t>
                  <ar>نَصْرًا</ar>
               </t>
               <t>
                  <ar>نَصْرَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="١٩٦٥٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يَا نَصْرُ نَصْرُ نَصْرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>نَصْرُ</ar>
               </t>
               <t>
                  <ar>نَصْرُ</ar>
               </t>
               <t>
                  <ar>نَصْرَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٤٧٤٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَيُّ فَتًى هَيْجَاْءَ أَنْتَ وَجاْرَها</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَيُّ</ar>
               </t>
               <t>
                  <ar>فَتًى</ar>
               </t>
               <t>
                  <ar>هَيْجَاْءَ</ar>
               </t>
               <t>
                  <ar>أَنْتَ</ar>
               </t>
               <t>
                  <ar>وَجاْرَها</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤١٢٠٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يَا دَاْرَ عَفْرَاْءَ وَدَاْرَ الْبَْخدَنِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>دَاْرَ</ar>
               </t>
               <t>
                  <ar>عَفْرَاْءَ</ar>
               </t>
               <t>
                  <ar>وَدَاْرَ</ar>
               </t>
               <t>
                  <ar>الْبَْخدَنِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
