<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="481">
   <verses id="٤٨٦٦٣" poet="هُدْبَةُ بْنُ خَشْرَمٍ">
      <verse>
         <hemistiche>
            <text>
               <ar>عَسَى اللهُ يُغْنِي عَنْ بِلَاْدِ ابْنِ قَاْدِرٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>عَسَى</ar>
               </t>
               <t>
                  <ar>اللهُ</ar>
               </t>
               <t>
                  <ar>يُغْنِي</ar>
               </t>
               <t>
                  <ar>عَنْ</ar>
               </t>
               <t>
                  <ar>بِلَاْدِ</ar>
               </t>
               <t>
                  <ar>ابْنِ</ar>
               </t>
               <t>
                  <ar>قَاْدِرٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>بِمُنْهَمِرٍ جَوْنِ الرَّبَاْبِ سَكُوْبِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بِمُنْهَمِرٍ</ar>
               </t>
               <t>
                  <ar>جَوْنِ</ar>
               </t>
               <t>
                  <ar>الرَّبَاْبِ</ar>
               </t>
               <t>
                  <ar>سَكُوْبِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
