   <verses id="٠٥٩٨٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَأَنْتَ مَكَاْنُكَ مِنْ وَائِلٍ</ar>
               <free/>
            </text>
            <igt>
