   <verses id="٧٧٣٢٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَوَرَدْنَ وَالعَيُّوْقُ مَقْعَدَ رَاْبِىءِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَوَرَدْنَ</ar>
               </t>
               <t>
                  <ar>وَالعَيُّوْقُ</ar>
               </t>
               <t>
                  <ar>مَقْعَدَ</ar>
               </t>
               <t>
                  <ar>رَاْبِىءِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>الضُّرَبَاْءِ خَلْفَ النَّجْمِ لَا يَتَتَلَّعُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>الضُّرَبَاْءِ</ar>
               </t>
               <t>
                  <ar>خَلْفَ</ar>
               </t>
               <t>
                  <ar>النَّجْمِ</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>يَتَتَلَّعُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٧٧١٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَإِنَّ بَنِي حَرْبٍ كَمَا قَدْ عَلِمْتُمُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَإِنَّ</ar>
               </t>
               <t>
                  <ar>بَنِي</ar>
               </t>
               <t>
                  <ar>حَرْبٍ</ar>
               </t>
               <t>
                  <ar>كَمَا</ar>
               </t>
               <t>
                  <ar>قَدْ</ar>
               </t>
               <t>
                  <ar>عَلِمْتُمُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>مَنَاْطَ الثُّرَيَّا قَدْ تَعَلَّتْ نُجُوْمُهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَنَاْطَ</ar>
               </t>
               <t>
                  <ar>الثُّرَيَّا</ar>
               </t>
               <t>
                  <ar>قَدْ</ar>
               </t>
               <t>
                  <ar>تَعَلَّتْ</ar>
               </t>
               <t>
                  <ar>نُجُوْمُهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٠٠٨٣" poet="">
