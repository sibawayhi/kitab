         <hemistiche>
            <text>
               <ar>أَنُصْبٌ لِلْمَنِيَّةِ تَعْتَرِيْهِمْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَنُصْبٌ</ar>
               </t>
               <t>
                  <ar>لِلْمَنِيَّةِ</ar>
               </t>
               <t>
                  <ar>تَعْتَرِيْهِمْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>رِجَاْلِي أَمْ هُمُ دَرَجَ السُّيُوْلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>رِجَاْلِي</ar>
