<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="217">
   <verses id="٦٧٩٥٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَأُمُّ أَوْ عَاْلٍ كَهَا أَوْ أَقْرَبَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَأُمُّ</ar>
               </t>
               <t>
                  <ar>أَوْ</ar>
               </t>
               <t>
                  <ar>عَاْلٍ</ar>
               </t>
               <t>
                  <ar>كَهَا</ar>
               </t>
               <t>
                  <ar>أَوْ</ar>
               </t>
               <t>
                  <ar>أَقْرَبَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٧٤٢٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَلَا تَرَى بَعْلًا وَلَا حَلَاْئِلَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَلَا</ar>
               </t>
               <t>
                  <ar>تَرَى</ar>
               </t>
               <t>
                  <ar>بَعْلًا</ar>
               </t>
               <t>
                  <ar>وَلَا</ar>
               </t>
               <t>
                  <ar>حَلَاْئِلَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>كَهُ وَلَا كَهُنَّ إِلَّا حَاْظِلَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَهُ</ar>
               </t>
               <t>
                  <ar>وَلَا</ar>
               </t>
               <t>
                  <ar>كَهُنَّ</ar>
               </t>
               <t>
                  <ar>إِلَّا</ar>
               </t>
               <t>
                  <ar>حَاْظِلَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
