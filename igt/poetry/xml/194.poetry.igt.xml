<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="194">
   <verses id="٥٣٨٧١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أُنِيْخَتْ فَأَلْقَتْ بَلْدَةً فَوْقَ بَلْدَةٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أُنِيْخَتْ</ar>
               </t>
               <t>
                  <ar>فَأَلْقَتْ</ar>
               </t>
               <t>
                  <ar>بَلْدَةً</ar>
               </t>
               <t>
                  <ar>فَوْقَ</ar>
               </t>
               <t>
                  <ar>بَلْدَةٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>قَلِيْلٍ بِهَا الْأَصْوَاْتُ إِلَّا بُغَاْمُهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>قَلِيْلٍ</ar>
               </t>
               <t>
                  <ar>بِهَا</ar>
               </t>
               <t>
                  <ar>الْأَصْوَاْتُ</ar>
               </t>
               <t>
                  <ar>إِلَّا</ar>
               </t>
               <t>
                  <ar>بُغَاْمُهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٤٨٤١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وإِذَا أُقْرِضْتَ قَرْضًا فَأَجْزِهِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وإِذَا</ar>
               </t>
               <t>
                  <ar>أُقْرِضْتَ</ar>
               </t>
               <t>
                  <ar>قَرْضًا</ar>
               </t>
               <t>
                  <ar>فَأَجْزِهِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>إِنَّمَا يَجْزِي الْفَتَى غَيْرُ الَجَمَلْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِنَّمَا</ar>
               </t>
               <t>
                  <ar>يَجْزِي</ar>
               </t>
               <t>
                  <ar>الْفَتَى</ar>
               </t>
               <t>
                  <ar>غَيْرُ</ar>
               </t>
               <t>
                  <ar>الَجَمَلْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٩٥٢٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَوْ كَاْنَ غَيْرِي سُلَيْمَى الْيَوْمَ غَيَّرَهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَوْ</ar>
               </t>
               <t>
                  <ar>كَاْنَ</ar>
               </t>
               <t>
                  <ar>غَيْرِي</ar>
               </t>
               <t>
                  <ar>سُلَيْمَى</ar>
               </t>
               <t>
                  <ar>الْيَوْمَ</ar>
               </t>
               <t>
                  <ar>غَيَّرَهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَقْعُ الَحَوَاْدِثِ إِلَّا الصَّاْرِمُ الذَّكَرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَقْعُ</ar>
               </t>
               <t>
                  <ar>الَحَوَاْدِثِ</ar>
               </t>
               <t>
                  <ar>إِلَّا</ar>
               </t>
               <t>
                  <ar>الصَّاْرِمُ</ar>
               </t>
               <t>
                  <ar>الذَّكَرُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٤٥٢٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَكُلُّ أَخٍ مُفَاْرِقُهُ أَخُوْهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَكُلُّ</ar>
               </t>
               <t>
                  <ar>أَخٍ</ar>
               </t>
               <t>
                  <ar>مُفَاْرِقُهُ</ar>
               </t>
               <t>
                  <ar>أَخُوْهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>لَعَمْرُ أَبِيْكَ إِلَّا الْفَرْقَدَاْنِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَعَمْرُ</ar>
               </t>
               <t>
                  <ar>أَبِيْكَ</ar>
               </t>
               <t>
                  <ar>إِلَّا</ar>
               </t>
               <t>
                  <ar>الْفَرْقَدَاْنِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٧٧٣٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَكُلُّ خَلِيْلٍ غَيْرُ هَاْضِمِ نَفْسِهِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَكُلُّ</ar>
               </t>
               <t>
                  <ar>خَلِيْلٍ</ar>
               </t>
               <t>
                  <ar>غَيْرُ</ar>
               </t>
               <t>
                  <ar>هَاْضِمِ</ar>
               </t>
               <t>
                  <ar>نَفْسِهِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>لِوْصَلِ خَلِيْلٍ صَاْرِمٌ أَوْ مُعَاْرِزُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لِوْصَلِ</ar>
               </t>
               <t>
                  <ar>خَلِيْلٍ</ar>
               </t>
               <t>
                  <ar>صَاْرِمٌ</ar>
               </t>
               <t>
                  <ar>أَوْ</ar>
               </t>
               <t>
                  <ar>مُعَاْرِزُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
