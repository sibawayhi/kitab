<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="422">
   <verses id="٥١٦٢٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كَنَهْوَرٌ كَاْنَ مِنَ أَعْقَاْبِ السُّمِيْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَنَهْوَرٌ</ar>
               </t>
               <t>
                  <ar>كَاْنَ</ar>
               </t>
               <t>
                  <ar>مِنَ</ar>
               </t>
               <t>
                  <ar>أَعْقَاْبِ</ar>
               </t>
               <t>
                  <ar>السُّمِيْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٤٧٧٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>طِرْنَ انْقِطَاْعَةَ أَوْتَاْرٍ مُحَظْرَبَةٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>طِرْنَ</ar>
               </t>
               <t>
                  <ar>انْقِطَاْعَةَ</ar>
               </t>
               <t>
                  <ar>أَوْتَاْرٍ</ar>
               </t>
               <t>
                  <ar>مُحَظْرَبَةٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فِيٌّ أَقْوُسٍ نَاْزَعْتْهَا أَيْمُنٌ شُمُلَاَْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فِيٌّ</ar>
               </t>
               <t>
                  <ar>أَقْوُسٍ</ar>
               </t>
               <t>
                  <ar>نَاْزَعْتْهَا</ar>
               </t>
               <t>
                  <ar>أَيْمُنٌ</ar>
               </t>
               <t>
                  <ar>شُمُلَاَْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٥١٥٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يَأْتِي لَهَا مِنْ أَيْمُنٍ وَأَشْمُلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَأْتِي</ar>
               </t>
               <t>
                  <ar>لَهَا</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>أَيْمُنٍ</ar>
               </t>
               <t>
                  <ar>وَأَشْمُلِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٦٥٠٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>خَنَاْثَى يَأْكُلُوْنَ التَّمّرْ لِيَسُوَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>خَنَاْثَى</ar>
               </t>
               <t>
                  <ar>يَأْكُلُوْنَ</ar>
               </t>
               <t>
                  <ar>التَّمّرْ</ar>
               </t>
               <t>
                  <ar>لِيَسُوَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>بِزَوْجَاْتٍ يَلِدْنَ وَلَا رِجَاْلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بِزَوْجَاْتٍ</ar>
               </t>
               <t>
                  <ar>يَلِدْنَ</ar>
               </t>
               <t>
                  <ar>وَلَا</ar>
               </t>
               <t>
                  <ar>رِجَاْلِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
