<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="033">
   <verses id="١٠٧٦٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَقَاْئِلَةٍ خَوْلانُ فَانْكِحْ فَتَاْتَهُمْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَقَاْئِلَةٍ</ar>
               </t>
               <t>
                  <ar>خَوْلانُ</ar>
               </t>
               <t>
                  <ar>فَانْكِحْ</ar>
               </t>
               <t>
                  <ar>فَتَاْتَهُمْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَأُكْرومَةُ الْحَيَّيْنِ خِلْوٌ كَمَا هِيَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَأُكْرومَةُ</ar>
               </t>
               <t>
                  <ar>الْحَيَّيْنِ</ar>
               </t>
               <t>
                  <ar>خِلْوٌ</ar>
               </t>
               <t>
                  <ar>كَمَا</ar>
               </t>
               <t>
                  <ar>هِيَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٦٥٤٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَرَوَاْحٌ مُوَدِّعٌ أَمْ بُكُوْرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَرَوَاْحٌ</ar>
               </t>
               <t>
                  <ar>مُوَدِّعٌ</ar>
               </t>
               <t>
                  <ar>أَمْ</ar>
               </t>
               <t>
                  <ar>بُكُوْرُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَنْتَ فَانْظُرْ لِأَيْ ذَاكَ تَصِيْرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَنْتَ</ar>
               </t>
               <t>
                  <ar>فَانْظُرْ</ar>
               </t>
               <t>
                  <ar>لِأَيْ</ar>
               </t>
               <t>
                  <ar>ذَاكَ</ar>
               </t>
               <t>
                  <ar>تَصِيْرُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨١٤٢٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَمِيْرَانِ كَانَا آخِيَانّىْ كِلَاهُمَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَمِيْرَانِ</ar>
               </t>
               <t>
                  <ar>كَانَا</ar>
               </t>
               <t>
                  <ar>آخِيَانّىْ</ar>
               </t>
               <t>
                  <ar>كِلَاهُمَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَكُلِا جَزَاهُ اللّٰهُ عَنِّىْ بِمَا فَعَلْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَكُلِا</ar>
               </t>
               <t>
                  <ar>جَزَاهُ</ar>
               </t>
               <t>
                  <ar>اللّٰهُ</ar>
               </t>
               <t>
                  <ar>عَنِّىْ</ar>
               </t>
               <t>
                  <ar>بِمَا</ar>
               </t>
               <t>
                  <ar>فَعَلْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٥٦٣٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَقَاْئِلَةِ خَوْلانُ فَانْكِحْ فَتَاْتَهُمْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَقَاْئِلَةِ</ar>
               </t>
               <t>
                  <ar>خَوْلانُ</ar>
               </t>
               <t>
                  <ar>فَانْكِحْ</ar>
               </t>
               <t>
                  <ar>فَتَاْتَهُمْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
