<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="500">
   <verses id="٨٤١٤٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَأَرَاْكَ تَفْرِي مَا خَلَقْتَ وَبَعْـ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَأَرَاْكَ</ar>
               </t>
               <t>
                  <ar>تَفْرِي</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>خَلَقْتَ</ar>
               </t>
               <t>
                  <ar>وَبَعْـ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>ـضُ الْقَوْمِ يَخْلُقُ ثُمَّ لَا يَفْرْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ـضُ</ar>
               </t>
               <t>
                  <ar>الْقَوْمِ</ar>
               </t>
               <t>
                  <ar>يَخْلُقُ</ar>
               </t>
               <t>
                  <ar>ثُمَّ</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>يَفْرْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
