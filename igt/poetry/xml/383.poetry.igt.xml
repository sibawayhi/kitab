<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="383">
   <verses id="٩٦٤٧٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَاْثٍ بِهِ الْأِشَاْءُ وَالْعُبْرِيُّ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَاْثٍ</ar>
               </t>
               <t>
                  <ar>بِهِ</ar>
               </t>
               <t>
                  <ar>الْأِشَاْءُ</ar>
               </t>
               <t>
                  <ar>وَالْعُبْرِيُّ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٧٤٥١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَتَعْرّفُوْنِي أَنّنِي أَنَا ذَاْكُمُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَتَعْرّفُوْنِي</ar>
               </t>
               <t>
                  <ar>أَنّنِي</ar>
               </t>
               <t>
                  <ar>أَنَا</ar>
               </t>
               <t>
                  <ar>ذَاْكُمُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>شَاْكٍ سِلَاْحِيٌّ فِي الْحَوَاْدِثِ مُعْلِمُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>شَاْكٍ</ar>
               </t>
               <t>
                  <ar>سِلَاْحِيٌّ</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>الْحَوَاْدِثِ</ar>
               </t>
               <t>
                  <ar>مُعْلِمُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="١٥٧٣٢" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لْقَدْ لَقِيَتْ قُرَيْظَةُ مَا سَآهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لْقَدْ</ar>
               </t>
               <t>
                  <ar>لَقِيَتْ</ar>
               </t>
               <t>
                  <ar>قُرَيْظَةُ</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>سَآهَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَحَلَّ بِدَاْرِهِمْ ذُلٌّ ذَلَيَلُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَحَلَّ</ar>
               </t>
               <t>
                  <ar>بِدَاْرِهِمْ</ar>
               </t>
               <t>
                  <ar>ذُلٌّ</ar>
               </t>
               <t>
                  <ar>ذَلَيَلُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨١٦٧٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَكُلُّ خَلِيْلٍ رَاْءَنِي فَهْوَ قَاْئِلٌ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَكُلُّ</ar>
               </t>
               <t>
                  <ar>خَلِيْلٍ</ar>
               </t>
               <t>
                  <ar>رَاْءَنِي</ar>
               </t>
               <t>
                  <ar>فَهْوَ</ar>
               </t>
               <t>
                  <ar>قَاْئِلٌ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>مِنَ أَجْلِكِ هٰذَا هَاْمَةُ الْيَوْمِ أَوْ غَدِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مِنَ</ar>
               </t>
               <t>
                  <ar>أَجْلِكِ</ar>
               </t>
               <t>
                  <ar>هٰذَا</ar>
               </t>
               <t>
                  <ar>هَاْمَةُ</ar>
               </t>
               <t>
                  <ar>الْيَوْمِ</ar>
               </t>
               <t>
                  <ar>أَوْ</ar>
               </t>
               <t>
                  <ar>غَدِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٦٢٢٢" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>سَاْلَتْ هُذَيْلٌ رَسُوْلَ اللَّهِ فَاْحِشَةً</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>سَاْلَتْ</ar>
               </t>
               <t>
                  <ar>هُذَيْلٌ</ar>
               </t>
               <t>
                  <ar>رَسُوْلَ</ar>
               </t>
               <t>
                  <ar>اللَّهِ</ar>
               </t>
               <t>
                  <ar>فَاْحِشَةً</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>ضَلَّتْ هّذَيْلٌ بِمَا جَاْءَتْ وَلَمْ تُصِبِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ضَلَّتْ</ar>
               </t>
               <t>
                  <ar>هّذَيْلٌ</ar>
               </t>
               <t>
                  <ar>بِمَا</ar>
               </t>
               <t>
                  <ar>جَاْءَتْ</ar>
               </t>
               <t>
                  <ar>وَلَمْ</ar>
               </t>
               <t>
                  <ar>تُصِبِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
