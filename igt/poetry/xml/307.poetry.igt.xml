<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="307">
   <verses id="٣٠١٥٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كَاْفًا وَمِيْمَيْنِ وَسِيْنًا طَاْسِمًا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَاْفًا</ar>
               </t>
               <t>
                  <ar>وَمِيْمَيْنِ</ar>
               </t>
               <t>
                  <ar>وَسِيْنًا</ar>
               </t>
               <t>
                  <ar>طَاْسِمًا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٠٢٧٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كَمَا بُيِّنَتْ كَاْفٌ تَلُوْحُ وَمِيْمُهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَمَا</ar>
               </t>
               <t>
                  <ar>بُيِّنَتْ</ar>
               </t>
               <t>
                  <ar>كَاْفٌ</ar>
               </t>
               <t>
                  <ar>تَلُوْحُ</ar>
               </t>
               <t>
                  <ar>وَمِيْمُهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥١٢٧٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَيْتَ شِعْرِيَ مُسَاْفِرَ بِنَ أَبِيْ عَمْرِوٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَيْتَ</ar>
               </t>
               <t>
                  <ar>شِعْرِيَ</ar>
               </t>
               <t>
                  <ar>مُسَاْفِرَ</ar>
               </t>
               <t>
                  <ar>بِنَ</ar>
               </t>
               <t>
                  <ar>أَبِيْ</ar>
               </t>
               <t>
                  <ar>عَمْرِوٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَلَيْتٌ يَقُوُلُهَا الْمَحْزُوْنُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَلَيْتٌ</ar>
               </t>
               <t>
                  <ar>يَقُوُلُهَا</ar>
               </t>
               <t>
                  <ar>الْمَحْزُوْنُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٦٥٠٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَيْتَ شِعْرِيَ وَأَيْنَ مِنِّيَ لَيْتَ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَيْتَ</ar>
               </t>
               <t>
                  <ar>شِعْرِيَ</ar>
               </t>
               <t>
                  <ar>وَأَيْنَ</ar>
               </t>
               <t>
                  <ar>مِنِّيَ</ar>
               </t>
               <t>
                  <ar>لَيْتَ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>إِنَّ لَيْتَا وَإِنَّ لَوّا عَنَاْءُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِنَّ</ar>
               </t>
               <t>
                  <ar>لَيْتَا</ar>
               </t>
               <t>
                  <ar>وَإِنَّ</ar>
               </t>
               <t>
                  <ar>لَوّا</ar>
               </t>
               <t>
                  <ar>عَنَاْءُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٢٧٠٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أُلَاْمُعَلَىلَوٍّ وَلَوْ كُنْتُ عَاْلِمْا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أُلَاْمُعَلَىلَوٍّ</ar>
               </t>
               <t>
                  <ar>وَلَوْ</ar>
               </t>
               <t>
                  <ar>كُنْتُ</ar>
               </t>
               <t>
                  <ar>عَاْلِمْا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>بِأَذْنَاْبِ لَوٍّ لَمْ تَفُتْنِيْ أَوَاْئِلُهْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بِأَذْنَاْبِ</ar>
               </t>
               <t>
                  <ar>لَوٍّ</ar>
               </t>
               <t>
                  <ar>لَمْ</ar>
               </t>
               <t>
                  <ar>تَفُتْنِيْ</ar>
               </t>
               <t>
                  <ar>أَوَاْئِلُهْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="١٨٢٩٢" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>تُكَتَّباْنِ فِي الْطَّرِيْقِ لَاْمَ أَلِفْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>تُكَتَّباْنِ</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>الْطَّرِيْقِ</ar>
               </t>
               <t>
                  <ar>لَاْمَ</ar>
               </t>
               <t>
                  <ar>أَلِفْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
