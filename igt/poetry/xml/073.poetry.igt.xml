<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="073">
   <verses id="١٧٣٧٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَبا مُنْذرٍ أَفْنَيْتَ فأسْتْبَقِ بَعْضَنَاْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَبا</ar>
               </t>
               <t>
                  <ar>مُنْذرٍ</ar>
               </t>
               <t>
                  <ar>أَفْنَيْتَ</ar>
               </t>
               <t>
                  <ar>فأسْتْبَقِ</ar>
               </t>
               <t>
                  <ar>بَعْضَنَاْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>حَنَاْنَيْكَ بَعْضُ الشرَّ أَهْوَنُ مِنْ بَعْضِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>حَنَاْنَيْكَ</ar>
               </t>
               <t>
                  <ar>بَعْضُ</ar>
               </t>
               <t>
                  <ar>الشرَّ</ar>
               </t>
               <t>
                  <ar>أَهْوَنُ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>بَعْضِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٢٤٥١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فقَاْلَت حَنَاْنٌ مَا أتى بك هَا هنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فقَاْلَت</ar>
               </t>
               <t>
                  <ar>حَنَاْنٌ</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>أتى</ar>
               </t>
               <t>
                  <ar>بك</ar>
               </t>
               <t>
                  <ar>هَا</ar>
               </t>
               <t>
                  <ar>هنَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩٠٨٥١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>إِذَا شُقَّ بُرْدٌ شُقَّ بَاْلُبْردِ مِثْلُه</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِذَا</ar>
               </t>
               <t>
                  <ar>شُقَّ</ar>
               </t>
               <t>
                  <ar>بُرْدٌ</ar>
               </t>
               <t>
                  <ar>شُقَّ</ar>
               </t>
               <t>
                  <ar>بَاْلُبْردِ</ar>
               </t>
               <t>
                  <ar>مِثْلُه</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>دَوَاْلَيْكَ جتّى لَيْسَ للبُرْدِ لَاْبِسُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>دَوَاْلَيْكَ</ar>
               </t>
               <t>
                  <ar>جتّى</ar>
               </t>
               <t>
                  <ar>لَيْسَ</ar>
               </t>
               <t>
                  <ar>للبُرْدِ</ar>
               </t>
               <t>
                  <ar>لَاْبِسُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٦٦٩٩٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>ضَرْبًا هَذا ذَيْكَ وَطَعْنًا وَخْضَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ضَرْبًا</ar>
               </t>
               <t>
                  <ar>هَذا</ar>
               </t>
               <t>
                  <ar>ذَيْكَ</ar>
               </t>
               <t>
                  <ar>وَطَعْنًا</ar>
               </t>
               <t>
                  <ar>وَخْضَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٢٨٥٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَهَدَمُوْا بَيْتَك لَا أَبَاْلَكَاْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَهَدَمُوْا</ar>
               </t>
               <t>
                  <ar>بَيْتَك</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>أَبَاْلَكَاْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَحسبوْا أَنَّكَ لَا أَخَاْلَكَاْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَحسبوْا</ar>
               </t>
               <t>
                  <ar>أَنَّكَ</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>أَخَاْلَكَاْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>وَأَنَا أَمشِى الدَّاْأَلَى حَوَاْلَكَاْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَأَنَا</ar>
               </t>
               <t>
                  <ar>أَمشِى</ar>
               </t>
               <t>
                  <ar>الدَّاْأَلَى</ar>
               </t>
               <t>
                  <ar>حَوَاْلَكَاْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٣١٩٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>دَعَوْتُ لِمَا نَاْبَنْى مِسْوَرًا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>دَعَوْتُ</ar>
               </t>
               <t>
                  <ar>لِمَا</ar>
               </t>
               <t>
                  <ar>نَاْبَنْى</ar>
               </t>
               <t>
                  <ar>مِسْوَرًا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَلَبَّى فَلبَّىْ يَدَىْ مِسْوَرِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَلَبَّى</ar>
               </t>
               <t>
                  <ar>فَلبَّىْ</ar>
               </t>
               <t>
                  <ar>يَدَىْ</ar>
               </t>
               <t>
                  <ar>مِسْوَرِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
