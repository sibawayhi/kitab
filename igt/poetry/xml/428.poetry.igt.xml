<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="428">
   <verses id="١٦٤٢٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>ظَهْرَاْهُمَا مِثْلُ ظُهُوْرِ التُّرْسَيْنْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ظَهْرَاْهُمَا</ar>
               </t>
               <t>
                  <ar>مِثْلُ</ar>
               </t>
               <t>
                  <ar>ظُهُوْرِ</ar>
               </t>
               <t>
                  <ar>التُّرْسَيْنْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩٢٤١٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>هُمَا نَفَثَا فِي فِيٌّ مِنْ فَمَوَيْهِمَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>هُمَا</ar>
               </t>
               <t>
                  <ar>نَفَثَا</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>فِيٌّ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>فَمَوَيْهِمَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>عَلِى النَّاْبِحِ الْعَاْوِي أَشَدَّ رَجَاْمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>عَلِى</ar>
               </t>
               <t>
                  <ar>النَّاْبِحِ</ar>
               </t>
               <t>
                  <ar>الْعَاْوِي</ar>
               </t>
               <t>
                  <ar>أَشَدَّ</ar>
               </t>
               <t>
                  <ar>رَجَاْمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٧٨٠٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>بُمَا فِي فُؤَاْدَيْنَا مِنْ الشًّوْقِ وَالْهَوَى</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بُمَا</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>فُؤَاْدَيْنَا</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>الشًّوْقِ</ar>
               </t>
               <t>
                  <ar>وَالْهَوَى</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَيُجْبَرُ مُنْهَاْضُ الْفُؤَاْدِ الْمُشَقَّفُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَيُجْبَرُ</ar>
               </t>
               <t>
                  <ar>مُنْهَاْضُ</ar>
               </t>
               <t>
                  <ar>الْفُؤَاْدِ</ar>
               </t>
               <t>
                  <ar>الْمُشَقَّفُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩٩٩٨٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كَأَنَّ خُصْيَيْهِ مِنَ التًّدَلْدُلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَأَنَّ</ar>
               </t>
               <t>
                  <ar>خُصْيَيْهِ</ar>
               </t>
               <t>
                  <ar>مِنَ</ar>
               </t>
               <t>
                  <ar>التًّدَلْدُلِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>ظَرْفُ عَجُوْزٍ فِيْهِ ثِنْتَا حَنْظَلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ظَرْفُ</ar>
               </t>
               <t>
                  <ar>عَجُوْزٍ</ar>
               </t>
               <t>
                  <ar>فِيْهِ</ar>
               </t>
               <t>
                  <ar>ثِنْتَا</ar>
               </t>
               <t>
                  <ar>حَنْظَلِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٠٢٧٧٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>قَدْ جَعَلَتْ مَيٌّ عَلَى الظِّرَاْرِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>قَدْ</ar>
               </t>
               <t>
                  <ar>جَعَلَتْ</ar>
               </t>
               <t>
                  <ar>مَيٌّ</ar>
               </t>
               <t>
                  <ar>عَلَى</ar>
               </t>
               <t>
                  <ar>الظِّرَاْرِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>خَمْسَ بَنَاْنٍ قَاْنِيْءِ الْأَظْفَاْرِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>خَمْسَ</ar>
               </t>
               <t>
                  <ar>بَنَاْنٍ</ar>
               </t>
               <t>
                  <ar>قَاْنِيْءِ</ar>
               </t>
               <t>
                  <ar>الْأَظْفَاْرِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
