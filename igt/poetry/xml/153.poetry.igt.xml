<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="153">
   <verses id="٧٢٩٠٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يَا لَبَكْرٍ أَنْشِرُوْا لِي كُلَيْبًا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>لَبَكْرٍ</ar>
               </t>
               <t>
                  <ar>أَنْشِرُوْا</ar>
               </t>
               <t>
                  <ar>لِي</ar>
               </t>
               <t>
                  <ar>كُلَيْبًا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>يَا لَبَكْرٍ أَيْنَ أَيْنَ الْفِرَاْرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>لَبَكْرٍ</ar>
               </t>
               <t>
                  <ar>أَيْنَ</ar>
               </t>
               <t>
                  <ar>أَيْنَ</ar>
               </t>
               <t>
                  <ar>الْفِرَاْرُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٦١٢٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَلَا يَا لَقَوْمِ لِطَيْفِ الْخَيَاْلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَلَا</ar>
               </t>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>لَقَوْمِ</ar>
               </t>
               <t>
                  <ar>لِطَيْفِ</ar>
               </t>
               <t>
                  <ar>الْخَيَاْلِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَرَّقَ مِنْ نَاْزِحٍ ذِي دَلَاْلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَرَّقَ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>نَاْزِحٍ</ar>
               </t>
               <t>
                  <ar>ذِي</ar>
               </t>
               <t>
                  <ar>دَلَاْلِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٣٢٦٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>تَكَنَّفَنِي الْوُشَاْةُ فَأَزْعَجُوْنِي</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>تَكَنَّفَنِي</ar>
               </t>
               <t>
                  <ar>الْوُشَاْةُ</ar>
               </t>
               <t>
                  <ar>فَأَزْعَجُوْنِي</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَيَا لَلنّاْسِ لِلْوَاْشِي الْمُطَاْعِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَيَا</ar>
               </t>
               <t>
                  <ar>لَلنّاْسِ</ar>
               </t>
               <t>
                  <ar>لِلْوَاْشِي</ar>
               </t>
               <t>
                  <ar>الْمُطَاْعِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٢٥٢٦" poet="">
