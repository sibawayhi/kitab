<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="047">
   <verses id="٣٣٦٥٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>تَرَاكِهَا مِنْ إِبْلٍ تَرَاكِهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>تَرَاكِهَا</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>إِبْلٍ</ar>
               </t>
               <t>
                  <ar>تَرَاكِهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٠٦٢٩٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>مَنَاْعِهَا مِنْ إِبِلٍ مَنَاْعِهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَنَاْعِهَا</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>إِبِلٍ</ar>
               </t>
               <t>
                  <ar>مَنَاْعِهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
