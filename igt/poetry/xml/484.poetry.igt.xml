<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="484">
   <verses id="٥٩٦٦٢" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>اضْرِبِ السَّاْقَيْنِ إِمُّكَ هَاْبِلٌ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>اضْرِبِ</ar>
               </t>
               <t>
                  <ar>السَّاْقَيْنِ</ar>
               </t>
               <t>
                  <ar>إِمُّكَ</ar>
               </t>
               <t>
                  <ar>هَاْبِلٌ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٣٥٢٢" poet="النُّعْمَاْنُ بْنُ بَشِيْرِ الْأَنْصَاْرِي">
      <verse>
         <hemistiche>
            <text>
               <ar>وَيْلِمِّهَا فِي هَوَاْءِ الْجَوِّ طَاْلِبَة</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَيْلِمِّهَا</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>هَوَاْءِ</ar>
               </t>
               <t>
                  <ar>الْجَوِّ</ar>
               </t>
               <t>
                  <ar>طَاْلِبَة</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَلَا كَهٰذَا الَّذِي فِي الْأَرْضِ مَطْلُوْبُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَلَا</ar>
               </t>
               <t>
                  <ar>كَهٰذَا</ar>
               </t>
               <t>
                  <ar>الَّذِي</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>الْأَرْضِ</ar>
               </t>
               <t>
                  <ar>مَطْلُوْبُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩١٠٩٢" poet="غَيْلَاْنُ">
      <verse>
         <hemistiche>
            <text>
               <ar>دَعْ ذَا وَعَجِّلْ ذَا وَأَلْحِقْنَا بِذَلْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>دَعْ</ar>
               </t>
               <t>
                  <ar>ذَا</ar>
               </t>
               <t>
                  <ar>وَعَجِّلْ</ar>
               </t>
               <t>
                  <ar>ذَا</ar>
               </t>
               <t>
                  <ar>وَأَلْحِقْنَا</ar>
               </t>
               <t>
                  <ar>بِذَلْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>بِالشَّحْمِ إِنَّا قَدْ مَلِلْنَاْهُ بَجَلْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بِالشَّحْمِ</ar>
               </t>
               <t>
                  <ar>إِنَّا</ar>
               </t>
               <t>
                  <ar>قَدْ</ar>
               </t>
               <t>
                  <ar>مَلِلْنَاْهُ</ar>
               </t>
               <t>
                  <ar>بَجَلْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٢٩٣٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَقَاْلَ فَرِيْقُ الْقَوْمِ لَمَّا نَشَدَتُهُمْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَقَاْلَ</ar>
               </t>
               <t>
                  <ar>فَرِيْقُ</ar>
               </t>
               <t>
                  <ar>الْقَوْمِ</ar>
               </t>
               <t>
                  <ar>لَمَّا</ar>
               </t>
               <t>
                  <ar>نَشَدَتُهُمْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>نَعَمْ وَفَرِيْقٌ لَيْمُنُ اللهِ مَا نَدْرِي</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>نَعَمْ</ar>
               </t>
               <t>
                  <ar>وَفَرِيْقٌ</ar>
               </t>
               <t>
                  <ar>لَيْمُنُ</ar>
               </t>
               <t>
                  <ar>اللهِ</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>نَدْرِي</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
