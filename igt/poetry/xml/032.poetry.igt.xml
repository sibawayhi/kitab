<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="032">
   <verses id="٨٩٤٣٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَكُلَّ عَامٍ نَعَمٌ تَحْوُونَهْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَكُلَّ</ar>
               </t>
               <t>
                  <ar>عَامٍ</ar>
               </t>
               <t>
                  <ar>نَعَمٌ</ar>
               </t>
               <t>
                  <ar>تَحْوُونَهْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>يُلْقِحُهُ قَوْمٌ وَتَنْتِجُوْنَهْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يُلْقِحُهُ</ar>
               </t>
               <t>
                  <ar>قَوْمٌ</ar>
               </t>
               <t>
                  <ar>وَتَنْتِجُوْنَهْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٠٣٥٥٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَفِيْ كُلِّ عَامٍ مَأْتَمٌ تَبْعَثُونَهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَفِيْ</ar>
               </t>
               <t>
                  <ar>كُلِّ</ar>
               </t>
               <t>
                  <ar>عَامٍ</ar>
               </t>
               <t>
                  <ar>مَأْتَمٌ</ar>
               </t>
               <t>
                  <ar>تَبْعَثُونَهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>عَلَىمِحْمَرٍ ثَوَّبْتُمُوهُ وَمَا رِضا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>عَلَىمِحْمَرٍ</ar>
               </t>
               <t>
                  <ar>ثَوَّبْتُمُوهُ</ar>
               </t>
               <t>
                  <ar>وَمَا</ar>
               </t>
               <t>
                  <ar>رِضا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٧٢٧٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَبَحْتَ حِمَىً تِهَامَةَ بَعْدَ نَجْدٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَبَحْتَ</ar>
               </t>
               <t>
                  <ar>حِمَىً</ar>
               </t>
               <t>
                  <ar>تِهَامَةَ</ar>
               </t>
               <t>
                  <ar>بَعْدَ</ar>
               </t>
               <t>
                  <ar>نَجْدٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَمَا شَيْءٌ حَمَيْتَ بِمُسْتَبَاْحِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَمَا</ar>
               </t>
               <t>
                  <ar>شَيْءٌ</ar>
               </t>
               <t>
                  <ar>حَمَيْتَ</ar>
               </t>
               <t>
                  <ar>بِمُسْتَبَاْحِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥١٠١٢" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَمَا أَدْرِىٓ أُغَيْرِهُمْ تَنَاْءٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَمَا</ar>
               </t>
               <t>
                  <ar>أَدْرِىٓ</ar>
               </t>
               <t>
                  <ar>أُغَيْرِهُمْ</ar>
               </t>
               <t>
                  <ar>تَنَاْءٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَطُوْلُ الْعَهْدِ أَمْ مَالٌ أَصَاْبُوا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَطُوْلُ</ar>
               </t>
               <t>
                  <ar>الْعَهْدِ</ar>
               </t>
               <t>
                  <ar>أَمْ</ar>
               </t>
               <t>
                  <ar>مَالٌ</ar>
               </t>
               <t>
                  <ar>أَصَاْبُوا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="١٠٤٢٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَا تَجْزَعِى إِنْ مُنْفِسا أَهْلَكْتُهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>تَجْزَعِى</ar>
               </t>
               <t>
                  <ar>إِنْ</ar>
               </t>
               <t>
                  <ar>مُنْفِسا</ar>
               </t>
               <t>
                  <ar>أَهْلَكْتُهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَإِذَا هَلَكْتُ فَعِنْدَ ذَلِكِ فَاْجَزَعَىْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَإِذَا</ar>
               </t>
               <t>
                  <ar>هَلَكْتُ</ar>
               </t>
               <t>
                  <ar>فَعِنْدَ</ar>
               </t>
               <t>
                  <ar>ذَلِكِ</ar>
               </t>
               <t>
                  <ar>فَاْجَزَعَىْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٤٥٣٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كُلُّهْ لَمْ أَصْنَعِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كُلُّهْ</ar>
               </t>
               <t>
                  <ar>لَمْ</ar>
               </t>
               <t>
                  <ar>أَصْنَعِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar/>
               <free/>
            </text>
            <igt/>
         </hemistiche>
      </verse>
   </verses>
</poetry>
