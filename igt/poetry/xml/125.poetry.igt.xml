<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="125">
   <verses id="١٦٩٠٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وكُلُّ خَلِيْلٍ غَيْرُ هاْضِمِ نَفْسِهِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وكُلُّ</ar>
               </t>
               <t>
                  <ar>خَلِيْلٍ</ar>
               </t>
               <t>
                  <ar>غَيْرُ</ar>
               </t>
               <t>
                  <ar>هاْضِمِ</ar>
               </t>
               <t>
                  <ar>نَفْسِهِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>لِوَصْلِ خَلِيْلٍ صَاْرَِمٌ أَوْ مُعاْرِزُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لِوَصْلِ</ar>
               </t>
               <t>
                  <ar>خَلِيْلٍ</ar>
               </t>
               <t>
                  <ar>صَاْرَِمٌ</ar>
               </t>
               <t>
                  <ar>أَوْ</ar>
               </t>
               <t>
                  <ar>مُعاْرِزُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٠٣٥٧١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كَأَنّا يَوْمَ قُرَّى إِنْـ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَأَنّا</ar>
               </t>
               <t>
                  <ar>يَوْمَ</ar>
               </t>
               <t>
                  <ar>قُرَّى</ar>
               </t>
               <t>
                  <ar>إِنْـ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>نَمَا نَقْتُلُ إِيَّاْنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>نَمَا</ar>
               </t>
               <t>
                  <ar>نَقْتُلُ</ar>
               </t>
               <t>
                  <ar>إِيَّاْنَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>قَتَلْنَا مِنْهُمُ كُلَّ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>قَتَلْنَا</ar>
               </t>
               <t>
                  <ar>مِنْهُمُ</ar>
               </t>
               <t>
                  <ar>كُلَّ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَتًى أَبْيَضَ حُسَّاْنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَتًى</ar>
               </t>
               <t>
                  <ar>أَبْيَضَ</ar>
               </t>
               <t>
                  <ar>حُسَّاْنَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩٥٣٢٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَلِهَتْ عَلَيْهِ كُلُّ مُعْصِفَةٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَلِهَتْ</ar>
               </t>
               <t>
                  <ar>عَلَيْهِ</ar>
               </t>
               <t>
                  <ar>كُلُّ</ar>
               </t>
               <t>
                  <ar>مُعْصِفَةٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>هَوْجَاْءُ لَيْسَ لِلُبِّهَا زَبْرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>هَوْجَاْءُ</ar>
               </t>
               <t>
                  <ar>لَيْسَ</ar>
               </t>
               <t>
                  <ar>لِلُبِّهَا</ar>
               </t>
               <t>
                  <ar>زَبْرُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
