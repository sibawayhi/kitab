<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="418">
   <verses id="٤٩٣٥٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لِكُلِّ عَيْشٍ قَدْ لَبِسْتُ أَثْوُبَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لِكُلِّ</ar>
               </t>
               <t>
                  <ar>عَيْشٍ</ar>
               </t>
               <t>
                  <ar>قَدْ</ar>
               </t>
               <t>
                  <ar>لَبِسْتُ</ar>
               </t>
               <t>
                  <ar>أَثْوُبَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٨٦٨٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَنَعْتُ أَعْيَاْرًا رَعَيْنَ الْخَنْزَرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَنَعْتُ</ar>
               </t>
               <t>
                  <ar>أَعْيَاْرًا</ar>
               </t>
               <t>
                  <ar>رَعَيْنَ</ar>
               </t>
               <t>
                  <ar>الْخَنْزَرَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَنْعَتُهُنَّ آبُرًا وَكَمَرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَنْعَتُهُنَّ</ar>
               </t>
               <t>
                  <ar>آبُرًا</ar>
               </t>
               <t>
                  <ar>وَكَمَرَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٢٠١٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يًّا أَضْبُعًا أَكَلَتْ آيَاْرَ أَحْمِرَةٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يًّا</ar>
               </t>
               <t>
                  <ar>أَضْبُعًا</ar>
               </t>
               <t>
                  <ar>أَكَلَتْ</ar>
               </t>
               <t>
                  <ar>آيَاْرَ</ar>
               </t>
               <t>
                  <ar>أَحْمِرَةٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَفِيّ الْبُطُوْنِ وَقَدْ رَاْحَتْ قَرَاْقِيْرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَفِيّ</ar>
               </t>
               <t>
                  <ar>الْبُطُوْنِ</ar>
               </t>
               <t>
                  <ar>وَقَدْ</ar>
               </t>
               <t>
                  <ar>رَاْحَتْ</ar>
               </t>
               <t>
                  <ar>قَرَاْقِيْرُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٥١٥٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَلَكِنَّنِي أَغْدُو عَلَيَّ مُفَاْضَةٌ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَلَكِنَّنِي</ar>
               </t>
               <t>
                  <ar>أَغْدُو</ar>
               </t>
               <t>
                  <ar>عَلَيَّ</ar>
               </t>
               <t>
                  <ar>مُفَاْضَةٌ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>دِلَاْصٌ كَأَعْيَاْنِ الْجَرَاْدِ الْمُنَظَّمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>دِلَاْصٌ</ar>
               </t>
               <t>
                  <ar>كَأَعْيَاْنِ</ar>
               </t>
               <t>
                  <ar>الْجَرَاْدِ</ar>
               </t>
               <t>
                  <ar>الْمُنَظَّمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٣٠٧٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يَقُوْمُ تَاْرَاْتٍ وَيَمْشِي تِيَرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَقُوْمُ</ar>
               </t>
               <t>
                  <ar>تَاْرَاْتٍ</ar>
               </t>
               <t>
                  <ar>وَيَمْشِي</ar>
               </t>
               <t>
                  <ar>تِيَرَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
