<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="075">
   <verses id="٢٨٨٦٢" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>مَقْذُوْفَةٍ بِدَخِيْسِ النَّحْضِ بَاْزِلُهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَقْذُوْفَةٍ</ar>
               </t>
               <t>
                  <ar>بِدَخِيْسِ</ar>
               </t>
               <t>
                  <ar>النَّحْضِ</ar>
               </t>
               <t>
                  <ar>بَاْزِلُهَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>لَهُ صَرِيِفٌ صَرِيْفَ الْقَعْوِ بِاْلْمَسَدِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَهُ</ar>
               </t>
               <t>
                  <ar>صَرِيِفٌ</ar>
               </t>
               <t>
                  <ar>صَرِيْفَ</ar>
               </t>
               <t>
                  <ar>الْقَعْوِ</ar>
               </t>
               <t>
                  <ar>بِاْلْمَسَدِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٧٣٨٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَهَا بَعْدَ إِسْنَاْدِ الْكَلِيْمِ وَهَدئِهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَهَا</ar>
               </t>
               <t>
                  <ar>بَعْدَ</ar>
               </t>
               <t>
                  <ar>إِسْنَاْدِ</ar>
               </t>
               <t>
                  <ar>الْكَلِيْمِ</ar>
               </t>
               <t>
                  <ar>وَهَدئِهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَرَنَّةِ مَّنْ يُبْكَى إِذَا كَاْنَ بَاْكِيًا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَرَنَّةِ</ar>
               </t>
               <t>
                  <ar>مَّنْ</ar>
               </t>
               <t>
                  <ar>يُبْكَى</ar>
               </t>
               <t>
                  <ar>إِذَا</ar>
               </t>
               <t>
                  <ar>كَاْنَ</ar>
               </t>
               <t>
                  <ar>بَاْكِيًا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>هَدَيْرٌ هَدِيْرَ الثَّوْرِ يَنْفُضُ رَأْسَهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>هَدَيْرٌ</ar>
               </t>
               <t>
                  <ar>هَدِيْرَ</ar>
               </t>
               <t>
                  <ar>الثَّوْرِ</ar>
               </t>
               <t>
                  <ar>يَنْفُضُ</ar>
               </t>
               <t>
                  <ar>رَأْسَهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>يَذُبُّ بِرَوْقَيْهِ الْكِلاْبَ الضَّوَاْرِيًا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَذُبُّ</ar>
               </t>
               <t>
                  <ar>بِرَوْقَيْهِ</ar>
               </t>
               <t>
                  <ar>الْكِلاْبَ</ar>
               </t>
               <t>
                  <ar>الضَّوَاْرِيًا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٣٦١٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>إِذَا رَأَتْنِى سَقَطَتْ أَبْصَاْرُهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِذَا</ar>
               </t>
               <t>
                  <ar>رَأَتْنِى</ar>
               </t>
               <t>
                  <ar>سَقَطَتْ</ar>
               </t>
               <t>
                  <ar>أَبْصَاْرُهَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>دَأْبَ بْكَاْرٍ شاْيَحتُ بْكَاْرِهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>دَأْبَ</ar>
               </t>
               <t>
                  <ar>بْكَاْرٍ</ar>
               </t>
               <t>
                  <ar>شاْيَحتُ</ar>
               </t>
               <t>
                  <ar>بْكَاْرِهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٢٧٤٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَوَّحَهَا مِنْ بَعْدَ بُدْنٍ وَسَنقُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَوَّحَهَا</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>بَعْدَ</ar>
               </t>
               <t>
                  <ar>بُدْنٍ</ar>
               </t>
               <t>
                  <ar>وَسَنقُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>تَضْمِيْرَكَ السَّاْبِقَ يُطْوَى لِلسَّبَقْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>تَضْمِيْرَكَ</ar>
               </t>
               <t>
                  <ar>السَّاْبِقَ</ar>
               </t>
               <t>
                  <ar>يُطْوَى</ar>
               </t>
               <t>
                  <ar>لِلسَّبَقْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٥٢٨٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>نَاْجٍ طَوَاْهُ الْأَيْنُ مِمَّا وَجَفا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>نَاْجٍ</ar>
               </t>
               <t>
                  <ar>طَوَاْهُ</ar>
               </t>
               <t>
                  <ar>الْأَيْنُ</ar>
               </t>
               <t>
                  <ar>مِمَّا</ar>
               </t>
               <t>
                  <ar>وَجَفا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>طَيَّ اللَّيَاْلِي زُلَفًا فَزُلَفًا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>طَيَّ</ar>
               </t>
               <t>
                  <ar>اللَّيَاْلِي</ar>
               </t>
               <t>
                  <ar>زُلَفًا</ar>
               </t>
               <t>
                  <ar>فَزُلَفًا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>سَمَاْوَةَ الْهِلاْلِ حَتَّى احْقَوْقَفًا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>سَمَاْوَةَ</ar>
               </t>
               <t>
                  <ar>الْهِلاْلِ</ar>
               </t>
               <t>
                  <ar>حَتَّى</ar>
               </t>
               <t>
                  <ar>احْقَوْقَفًا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩٠٦٧٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>مَا إِنْ يَمَسُّ الْأَرْضَ إِلَّا مَنْكِبٌ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>إِنْ</ar>
               </t>
               <t>
                  <ar>يَمَسُّ</ar>
               </t>
               <t>
                  <ar>الْأَرْضَ</ar>
               </t>
               <t>
                  <ar>إِلَّا</ar>
               </t>
               <t>
                  <ar>مَنْكِبٌ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>مِنْهُ وَحَرْفُ السَّاْقِ طَيَّ الْمِحْمَلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مِنْهُ</ar>
               </t>
               <t>
                  <ar>وَحَرْفُ</ar>
               </t>
               <t>
                  <ar>السَّاْقِ</ar>
               </t>
               <t>
                  <ar>طَيَّ</ar>
               </t>
               <t>
                  <ar>الْمِحْمَلِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
