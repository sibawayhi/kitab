<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="201">
   <verses id="٣٤٢٥٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كَأَنَّكَ مِنْ جِمَاْلِ بَنِى أُقَيْشٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَأَنَّكَ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>جِمَاْلِ</ar>
               </t>
               <t>
                  <ar>بَنِى</ar>
               </t>
               <t>
                  <ar>أُقَيْشٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>يُقَعْقَعُ خَلْفَ رِجْلَيْهِ بِشَنِّ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يُقَعْقَعُ</ar>
               </t>
               <t>
                  <ar>خَلْفَ</ar>
               </t>
               <t>
                  <ar>رِجْلَيْهِ</ar>
               </t>
               <t>
                  <ar>بِشَنِّ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٩١٠٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لُو قُلْتَ مَا فِى قَوْمِهَا لَمْ تِيْثَمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لُو</ar>
               </t>
               <t>
                  <ar>قُلْتَ</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>فِى</ar>
               </t>
               <t>
                  <ar>قَوْمِهَا</ar>
               </t>
               <t>
                  <ar>لَمْ</ar>
               </t>
               <t>
                  <ar>تِيْثَمِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>يَفَضُلَهُا فِى حَسَبٍ وَمِيْسَمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَفَضُلَهُا</ar>
               </t>
               <t>
                  <ar>فِى</ar>
               </t>
               <t>
                  <ar>حَسَبٍ</ar>
               </t>
               <t>
                  <ar>وَمِيْسَمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="١٤٣٣٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>ومَا الدَّهْرُ إلَّاّْ تَاْرَتَاْنِ فَمِنْهُمَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ومَا</ar>
               </t>
               <t>
                  <ar>الدَّهْرُ</ar>
               </t>
               <t>
                  <ar>إلَّاّْ</ar>
               </t>
               <t>
                  <ar>تَاْرَتَاْنِ</ar>
               </t>
               <t>
                  <ar>فَمِنْهُمَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَمُوْتُ وَأُخْرَى أَبْتَغَى الْعَيْشَ أَكْدَحُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَمُوْتُ</ar>
               </t>
               <t>
                  <ar>وَأُخْرَى</ar>
               </t>
               <t>
                  <ar>أَبْتَغَى</ar>
               </t>
               <t>
                  <ar>الْعَيْشَ</ar>
               </t>
               <t>
                  <ar>أَكْدَحُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٨٥٤٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>بَعْدُ اللَّتْيَّا وَالَّتِيَّا وَالتِّيْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بَعْدُ</ar>
               </t>
               <t>
                  <ar>اللَّتْيَّا</ar>
               </t>
               <t>
                  <ar>وَالَّتِيَّا</ar>
               </t>
               <t>
                  <ar>وَالتِّيْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
