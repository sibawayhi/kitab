<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="400">
   <verses id="٣٥٩٠٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>هِيَ ابْنَتُكُمْ وَأُخْتُكُمُ زَعَمْتُمْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>هِيَ</ar>
               </t>
               <t>
                  <ar>ابْنَتُكُمْ</ar>
               </t>
               <t>
                  <ar>وَأُخْتُكُمُ</ar>
               </t>
               <t>
                  <ar>زَعَمْتُمْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>لِـثَعْلَبَةَ بْنِ نَوْفَلٍ ابْنِ جَسْرِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لِـثَعْلَبَةَ</ar>
               </t>
               <t>
                  <ar>بْنِ</ar>
               </t>
               <t>
                  <ar>نَوْفَلٍ</ar>
               </t>
               <t>
                  <ar>ابْنِ</ar>
               </t>
               <t>
                  <ar>جَسْرِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٤٣٢٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>جَاْرِيَةٌ مِنْ قَيْسٍ ابْنِ ثَعْلَبَهْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>جَاْرِيَةٌ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>قَيْسٍ</ar>
               </t>
               <t>
                  <ar>ابْنِ</ar>
               </t>
               <t>
                  <ar>ثَعْلَبَهْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٠١١٨٣" poet="الْفَرَزْدَقُ">
      <verse>
         <hemistiche>
            <text>
               <ar>مَاْزِلْتُ أُغْلِقُ أَبْوَاْبًا وَأَفْتَحُهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَاْزِلْتُ</ar>
               </t>
               <t>
                  <ar>أُغْلِقُ</ar>
               </t>
               <t>
                  <ar>أَبْوَاْبًا</ar>
               </t>
               <t>
                  <ar>وَأَفْتَحُهَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>حَتَّى أَتَيْتُ أَبَا عَمْرِو بْنَ عَمَّاْرِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>حَتَّى</ar>
               </t>
               <t>
                  <ar>أَتَيْتُ</ar>
               </t>
               <t>
                  <ar>أَبَا</ar>
               </t>
               <t>
                  <ar>عَمْرِو</ar>
               </t>
               <t>
                  <ar>بْنَ</ar>
               </t>
               <t>
                  <ar>عَمَّاْرِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٣٦٢٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَلَمْ أَجْبُنْ وَلَمْ أَنْكُلْ وَلٰكِنْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَلَمْ</ar>
               </t>
               <t>
                  <ar>أَجْبُنْ</ar>
               </t>
               <t>
                  <ar>وَلَمْ</ar>
               </t>
               <t>
                  <ar>أَنْكُلْ</ar>
               </t>
               <t>
                  <ar>وَلٰكِنْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>يَمَمْتُ بِهَا أَبَا صَخْرِ بْنَ عَمْرِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَمَمْتُ</ar>
               </t>
               <t>
                  <ar>بِهَا</ar>
               </t>
               <t>
                  <ar>أَبَا</ar>
               </t>
               <t>
                  <ar>صَخْرِ</ar>
               </t>
               <t>
                  <ar>بْنَ</ar>
               </t>
               <t>
                  <ar>عَمْرِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
