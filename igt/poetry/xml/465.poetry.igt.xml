   <verses id="٨٤٧١٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَنّ الْمُوَقَّى مِثْلُ مَا وُقِّيْتُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَنّ</ar>
               </t>
               <t>
                  <ar>الْمُوَقَّى</ar>
