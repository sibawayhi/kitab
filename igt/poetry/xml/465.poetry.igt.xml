<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="465">
   <verses id="١٦٥٣١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>الْحَمْدُ لِلَّهِ مُمْسَّاْنًا وَمُصْبَحَنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>الْحَمْدُ</ar>
               </t>
               <t>
                  <ar>لِلَّهِ</ar>
               </t>
               <t>
                  <ar>مُمْسَّاْنًا</ar>
               </t>
               <t>
                  <ar>وَمُصْبَحَنَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>بِاْلْخَيْرِ صُبّحَنَا رَبِّي وَمَسَاْنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بِاْلْخَيْرِ</ar>
               </t>
               <t>
                  <ar>صُبّحَنَا</ar>
               </t>
               <t>
                  <ar>رَبِّي</ar>
               </t>
               <t>
                  <ar>وَمَسَاْنَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٩٠٧٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أُقَاْتِلُ حَتّى لَا أَرَى لِي مُقَاْتَلًا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أُقَاْتِلُ</ar>
               </t>
               <t>
                  <ar>حَتّى</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>أَرَى</ar>
               </t>
               <t>
                  <ar>لِي</ar>
               </t>
               <t>
                  <ar>مُقَاْتَلًا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَأَنْجُو إذَا غُمّ الْجَبَاْنُ مِنْ الْكَرْبِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَأَنْجُو</ar>
               </t>
               <t>
                  <ar>إذَا</ar>
               </t>
               <t>
                  <ar>غُمّ</ar>
               </t>
               <t>
                  <ar>الْجَبَاْنُ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>الْكَرْبِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٨٦٧٨" poet="">
