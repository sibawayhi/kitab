<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="143">
   <verses id="٦٩٢٦٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَنَا مِرْفَدٌ سَبْعُوْنَ أَلْفَ مُدَجَّجٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَنَا</ar>
               </t>
               <t>
                  <ar>مِرْفَدٌ</ar>
               </t>
               <t>
                  <ar>سَبْعُوْنَ</ar>
               </t>
               <t>
                  <ar>أَلْفَ</ar>
               </t>
               <t>
                  <ar>مُدَجَّجٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَهَلْ فِي مَعَدٍّ فَوْقَ ذٰلِكَ مِرْفَدَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَهَلْ</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>مَعَدٍّ</ar>
               </t>
               <t>
                  <ar>فَوْقَ</ar>
               </t>
               <t>
                  <ar>ذٰلِكَ</ar>
               </t>
               <t>
                  <ar>مِرْفَدَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
