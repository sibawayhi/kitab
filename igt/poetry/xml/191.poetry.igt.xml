<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="191">
   <verses id="٨٥٩٠٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَلَا عَيْبَ فِيْهِمْ غَيْرَ أَنَّ سُيُوْفَهُمُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَلَا</ar>
               </t>
               <t>
                  <ar>عَيْبَ</ar>
               </t>
               <t>
                  <ar>فِيْهِمْ</ar>
               </t>
               <t>
                  <ar>غَيْرَ</ar>
               </t>
               <t>
                  <ar>أَنَّ</ar>
               </t>
               <t>
                  <ar>سُيُوْفَهُمُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>بِهِنَّ فُلُوْلٌ مِنْ قِرَاْعِ الْكَتَاْئِبِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بِهِنَّ</ar>
               </t>
               <t>
                  <ar>فُلُوْلٌ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>قِرَاْعِ</ar>
               </t>
               <t>
                  <ar>الْكَتَاْئِبِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٠٥٦٤١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَتًى كَمُلَتْ خَيْرَاْتُهُ غَيْرَ أَنَّهَ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَتًى</ar>
               </t>
               <t>
                  <ar>كَمُلَتْ</ar>
               </t>
               <t>
                  <ar>خَيْرَاْتُهُ</ar>
               </t>
               <t>
                  <ar>غَيْرَ</ar>
               </t>
               <t>
                  <ar>أَنَّهَ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>جَوَاْدٌ فَلَا يُبْقِي مِنَ الْمَاْلِ بَاْقِيَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>جَوَاْدٌ</ar>
               </t>
               <t>
                  <ar>فَلَا</ar>
               </t>
               <t>
                  <ar>يُبْقِي</ar>
               </t>
               <t>
                  <ar>مِنَ</ar>
               </t>
               <t>
                  <ar>الْمَاْلِ</ar>
               </t>
               <t>
                  <ar>بَاْقِيَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٠٧٧١٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَمَا سَجَنُوْنِي غَيْرَ أَنِّي ابْنُ غَاْلِبٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَمَا</ar>
               </t>
               <t>
                  <ar>سَجَنُوْنِي</ar>
               </t>
               <t>
                  <ar>غَيْرَ</ar>
               </t>
               <t>
                  <ar>أَنِّي</ar>
               </t>
               <t>
                  <ar>ابْنُ</ar>
               </t>
               <t>
                  <ar>غَاْلِبٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَأَنِّي مِنَ الْأَثْرَيْنِ غَيْرِ الزَّعَاْنِفِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَأَنِّي</ar>
               </t>
               <t>
                  <ar>مِنَ</ar>
               </t>
               <t>
                  <ar>الْأَثْرَيْنِ</ar>
               </t>
               <t>
                  <ar>غَيْرِ</ar>
               </t>
               <t>
                  <ar>الزَّعَاْنِفِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٠٤٥١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>مَنْ كَاْنَ أَشْرَكَ فِي تَفَرُّقِ فَاْلِجٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَنْ</ar>
               </t>
               <t>
                  <ar>كَاْنَ</ar>
               </t>
               <t>
                  <ar>أَشْرَكَ</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>تَفَرُّقِ</ar>
               </t>
               <t>
                  <ar>فَاْلِجٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>فَلَبُوْنُهُ جَرِبَتْ مَعًا وَأَغَدَّتِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَلَبُوْنُهُ</ar>
               </t>
               <t>
                  <ar>جَرِبَتْ</ar>
               </t>
               <t>
                  <ar>مَعًا</ar>
               </t>
               <t>
                  <ar>وَأَغَدَّتِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>إِلَّا كَنَاْشِرَةَ الَّذِي ضَيَّعْتُمُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِلَّا</ar>
               </t>
               <t>
                  <ar>كَنَاْشِرَةَ</ar>
               </t>
               <t>
                  <ar>الَّذِي</ar>
               </t>
               <t>
                  <ar>ضَيَّعْتُمُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>كَالْغُصْنِ فِي غُلْوَاْئِهِ الْمُتَنَبَّتِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَالْغُصْنِ</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>غُلْوَاْئِهِ</ar>
               </t>
               <t>
                  <ar>الْمُتَنَبَّتِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٦١٢٢٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَوْلَا ابْنُ حَاْرِثَةَ الْأَمِيْرُ لَقَدْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَوْلَا</ar>
               </t>
               <t>
                  <ar>ابْنُ</ar>
               </t>
               <t>
                  <ar>حَاْرِثَةَ</ar>
               </t>
               <t>
                  <ar>الْأَمِيْرُ</ar>
               </t>
               <t>
                  <ar>لَقَدْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَغْضَيْتَ مِنْ شَتْمِي عَلَى رَغْمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَغْضَيْتَ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>شَتْمِي</ar>
               </t>
               <t>
                  <ar>عَلَى</ar>
               </t>
               <t>
                  <ar>رَغْمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>إِلَّا كَمُعْرِضٍ الْمُحَسِّرِ بَكْرَهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِلَّا</ar>
               </t>
               <t>
                  <ar>كَمُعْرِضٍ</ar>
               </t>
               <t>
                  <ar>الْمُحَسِّرِ</ar>
               </t>
               <t>
                  <ar>بَكْرَهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>عَمْدًا يُسَبِّبُنِي عَلَى الظُّلْمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>عَمْدًا</ar>
               </t>
               <t>
                  <ar>يُسَبِّبُنِي</ar>
               </t>
               <t>
                  <ar>عَلَى</ar>
               </t>
               <t>
                  <ar>الظُّلْمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
