<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="491">
   <verses id="٦٣١٠٢" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يًّا أَيُّهَا النَّاْسُ أَلَاَْ هَلُمَّهْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يًّا</ar>
               </t>
               <t>
                  <ar>أَيُّهَا</ar>
               </t>
               <t>
                  <ar>النَّاْسُ</ar>
               </t>
               <t>
                  <ar>أَلَاَْ</ar>
               </t>
               <t>
                  <ar>هَلُمَّهْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٢٢٣٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَيَقُلْنَ شَيْبٌ قَدْ عَلَاْكَ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَيَقُلْنَ</ar>
               </t>
               <t>
                  <ar>شَيْبٌ</ar>
               </t>
               <t>
                  <ar>قَدْ</ar>
               </t>
               <t>
                  <ar>عَلَاْكَ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَقَدْ كَبِرَتَ فَقُلْتُ إنّهْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَقَدْ</ar>
               </t>
               <t>
                  <ar>كَبِرَتَ</ar>
               </t>
               <t>
                  <ar>فَقُلْتُ</ar>
               </t>
               <t>
                  <ar>إنّهْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
