<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="256">
   <verses id="٠٣١٣٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>عَاْوِدْ هَرَاْةَ وَإِنْ مَعْمُوْرُهَا خَرِبَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>عَاْوِدْ</ar>
               </t>
               <t>
                  <ar>هَرَاْةَ</ar>
               </t>
               <t>
                  <ar>وَإِنْ</ar>
               </t>
               <t>
                  <ar>مَعْمُوْرُهَا</ar>
               </t>
               <t>
                  <ar>خَرِبَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٣٠٧٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَمَتَى وَاْغِلٌ يَنُبْهُمْ يُحْيّوْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَمَتَى</ar>
               </t>
               <t>
                  <ar>وَاْغِلٌ</ar>
               </t>
               <t>
                  <ar>يَنُبْهُمْ</ar>
               </t>
               <t>
                  <ar>يُحْيّوْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>هُ وَتُعْطَفْ عَلَيْهِ كَأْسُ السَّاْقِي</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>هُ</ar>
               </t>
               <t>
                  <ar>وَتُعْطَفْ</ar>
               </t>
               <t>
                  <ar>عَلَيْهِ</ar>
               </t>
               <t>
                  <ar>كَأْسُ</ar>
               </t>
               <t>
                  <ar>السَّاْقِي</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٢٠٣٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>صَعْدَةٌ نَاْبِتَةٌ فِي حَاْئِرٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>صَعْدَةٌ</ar>
               </t>
               <t>
                  <ar>نَاْبِتَةٌ</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>حَاْئِرٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَيْنَمَا الرّيْحُ تُمَيّلْهَا تَمِلْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَيْنَمَا</ar>
               </t>
               <t>
                  <ar>الرّيْحُ</ar>
               </t>
               <t>
                  <ar>تُمَيّلْهَا</ar>
               </t>
               <t>
                  <ar>تَمِلْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٠٧٥٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>اللّهُ يَشْكُرُهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>اللّهُ</ar>
               </t>
               <t>
                  <ar>يَشْكُرُهَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٣٩٤٧١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَمَنْ نَحْنُ نُؤْمِنْهُ يَبِتْ وَهُوَ آمِنٌ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَمَنْ</ar>
               </t>
               <t>
                  <ar>نَحْنُ</ar>
               </t>
               <t>
                  <ar>نُؤْمِنْهُ</ar>
               </t>
               <t>
                  <ar>يَبِتْ</ar>
               </t>
               <t>
                  <ar>وَهُوَ</ar>
               </t>
               <t>
                  <ar>آمِنٌ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَمَنْ لَا نُجِرْهُ يُمْسِ مِنّا مُفْزّعَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَمَنْ</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>نُجِرْهُ</ar>
               </t>
               <t>
                  <ar>يُمْسِ</ar>
               </t>
               <t>
                  <ar>مِنّا</ar>
               </t>
               <t>
                  <ar>مُفْزّعَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
