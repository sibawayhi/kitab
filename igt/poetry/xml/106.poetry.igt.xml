<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="106">
   <verses id="٧٩٣٦٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَاْرْتَشْنَ حِيْنَ أَرَدْنَ أَنْ يَرْمِيْنَنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَاْرْتَشْنَ</ar>
               </t>
               <t>
                  <ar>حِيْنَ</ar>
               </t>
               <t>
                  <ar>أَرَدْنَ</ar>
               </t>
               <t>
                  <ar>أَنْ</ar>
               </t>
               <t>
                  <ar>يَرْمِيْنَنَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>نَبْلِا بِلَا رِيْشٍ وَلَا بِقِدَاْحِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>نَبْلِا</ar>
               </t>
               <t>
                  <ar>بِلَا</ar>
               </t>
               <t>
                  <ar>رِيْشٍ</ar>
               </t>
               <t>
                  <ar>وَلَا</ar>
               </t>
               <t>
                  <ar>بِقِدَاْحِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>وَنَظَرْنَ مِنْ خَلَلِ الْخُدُوْرِ بِأَعْيُنٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَنَظَرْنَ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>خَلَلِ</ar>
               </t>
               <t>
                  <ar>الْخُدُوْرِ</ar>
               </t>
               <t>
                  <ar>بِأَعْيُنٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>مَرْضَى مُخَاْلِطِهَا الْسَّقَاْمُ صِحَاْحِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَرْضَى</ar>
               </t>
               <t>
                  <ar>مُخَاْلِطِهَا</ar>
               </t>
               <t>
                  <ar>الْسَّقَاْمُ</ar>
               </t>
               <t>
                  <ar>صِحَاْحِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٩٦٠١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>حَمَيْنَ الْعَرَاْقِيْبَ الْعَصَا وَتَرَكْنَهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>حَمَيْنَ</ar>
               </t>
               <t>
                  <ar>الْعَرَاْقِيْبَ</ar>
               </t>
               <t>
                  <ar>الْعَصَا</ar>
               </t>
               <t>
                  <ar>وَتَرَكْنَهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>بِهِ نَفَسٌ عَاْلٍ مُخَاْلِطُهُ بُهْرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بِهِ</ar>
               </t>
               <t>
                  <ar>نَفَسٌ</ar>
               </t>
               <t>
                  <ar>عَاْلٍ</ar>
               </t>
               <t>
                  <ar>مُخَاْلِطُهُ</ar>
               </t>
               <t>
                  <ar>بُهْرُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
