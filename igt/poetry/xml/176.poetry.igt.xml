<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="176">
   <verses id="١٣١٩٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كِلِيْنِى لِهَمٍّ يَا أُمَيْمَةَ نَاْصِبِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كِلِيْنِى</ar>
               </t>
               <t>
                  <ar>لِهَمٍّ</ar>
               </t>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>أُمَيْمَةَ</ar>
               </t>
               <t>
                  <ar>نَاْصِبِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وَلَيْلٍ أقَاسيه بِطَيْءِ الْكَوَاْكِبِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَلَيْلٍ</ar>
               </t>
               <t>
                  <ar>أقَاسيه</ar>
               </t>
               <t>
                  <ar>بِطَيْءِ</ar>
               </t>
               <t>
                  <ar>الْكَوَاْكِبِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٢٣٨٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يَا بُؤْسَ لِلْجَهْلِ ضَرّاْرًا لِأَقْوَاْمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>بُؤْسَ</ar>
               </t>
               <t>
                  <ar>لِلْجَهْلِ</ar>
               </t>
               <t>
                  <ar>ضَرّاْرًا</ar>
               </t>
               <t>
                  <ar>لِأَقْوَاْمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٧٠٠٢" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَقَدْ مَاْتَ شَمَّاْخٌ وَمَاْتَ مُزَرِّدٌ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَقَدْ</ar>
               </t>
               <t>
                  <ar>مَاْتَ</ar>
               </t>
               <t>
                  <ar>شَمَّاْخٌ</ar>
               </t>
               <t>
                  <ar>وَمَاْتَ</ar>
               </t>
               <t>
                  <ar>مُزَرِّدٌ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>وأَىُّ كَرِيْمٍ لَا أَبَاْكَ يُمَّتَعُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وأَىُّ</ar>
               </t>
               <t>
                  <ar>كَرِيْمٍ</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>أَبَاْكَ</ar>
               </t>
               <t>
                  <ar>يُمَّتَعُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٢٥٥٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كَأَنَّ أَصْوَاْتَ مِنْ إِيْغَاْلِهِنَّ بِنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَأَنَّ</ar>
               </t>
               <t>
                  <ar>أَصْوَاْتَ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>إِيْغَاْلِهِنَّ</ar>
               </t>
               <t>
                  <ar>بِنَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>أَوَاْخِرِ الْمَيْسِ أَصْوَاْتُ الْفَرَاْرَيِجِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَوَاْخِرِ</ar>
               </t>
               <t>
                  <ar>الْمَيْسِ</ar>
               </t>
               <t>
                  <ar>أَصْوَاْتُ</ar>
               </t>
               <t>
                  <ar>الْفَرَاْرَيِجِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٩٧٢٣" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>أَبِى الْإِسْلَاْمُ لَا أَبَ لِي سِوَاْهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>أَبِى</ar>
               </t>
               <t>
                  <ar>الْإِسْلَاْمُ</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>أَبَ</ar>
               </t>
               <t>
                  <ar>لِي</ar>
               </t>
               <t>
                  <ar>سِوَاْهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>إِذَا افْتَخَرُوْا بِقَيْسٍ أَوْ تَمَيْمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِذَا</ar>
               </t>
               <t>
                  <ar>افْتَخَرُوْا</ar>
               </t>
               <t>
                  <ar>بِقَيْسٍ</ar>
               </t>
               <t>
                  <ar>أَوْ</ar>
               </t>
               <t>
                  <ar>تَمَيْمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٣٠٩٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَا أَبَ وَابْنًا مِثْلُ مَرْوَاْنَ وَابْنِهِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>أَبَ</ar>
               </t>
               <t>
                  <ar>وَابْنًا</ar>
               </t>
               <t>
                  <ar>مِثْلُ</ar>
               </t>
               <t>
                  <ar>مَرْوَاْنَ</ar>
               </t>
               <t>
                  <ar>وَابْنِهِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>إِذَا هُوَ بِالْمَجْدِ ارْتَدَى وَتَأَزَّرَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِذَا</ar>
               </t>
               <t>
                  <ar>هُوَ</ar>
               </t>
               <t>
                  <ar>بِالْمَجْدِ</ar>
               </t>
               <t>
                  <ar>ارْتَدَى</ar>
               </t>
               <t>
                  <ar>وَتَأَزَّرَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٦٣٩٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>لَا نَسَبَ الْيَوْمَ وَلَا خُلّةً</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>نَسَبَ</ar>
               </t>
               <t>
                  <ar>الْيَوْمَ</ar>
               </t>
               <t>
                  <ar>وَلَا</ar>
               </t>
               <t>
                  <ar>خُلّةً</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>اتَّسَعَ الْخَرْقُ عَلَى الرَّاْقِعِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>اتَّسَعَ</ar>
               </t>
               <t>
                  <ar>الْخَرْقُ</ar>
               </t>
               <t>
                  <ar>عَلَى</ar>
               </t>
               <t>
                  <ar>الرَّاْقِعِ</ar>
               </t>
            </igt>
