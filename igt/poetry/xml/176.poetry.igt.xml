      </verse>
   </verses>
   <verses id="٦٢٧٦٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يَا رُبَّ مِثْلِكِ فِى النِّسَاْءِ غَرِيْرَةٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>رُبَّ</ar>
               </t>
               <t>
                  <ar>مِثْلِكِ</ar>
               </t>
               <t>
                  <ar>فِى</ar>
               </t>
               <t>
                  <ar>النِّسَاْءِ</ar>
               </t>
               <t>
                  <ar>غَرِيْرَةٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>بَيْضَاْءَ قَدْ مَتَّعْتُهَا بِطَلَاْقِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>بَيْضَاْءَ</ar>
               </t>
               <t>
                  <ar>قَدْ</ar>
               </t>
               <t>
                  <ar>مَتَّعْتُهَا</ar>
               </t>
               <t>
                  <ar>بِطَلَاْقِ</ar>
               </t>
            </igt>
