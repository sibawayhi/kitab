<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="566">
   <verses id="٤٨٤٤٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وإنِّي بِمَا كَلَّفَتْنِي عَشِيْرَتَيْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وإنِّي</ar>
               </t>
               <t>
                  <ar>بِمَا</ar>
               </t>
               <t>
                  <ar>كَلَّفَتْنِي</ar>
               </t>
               <t>
                  <ar>عَشِيْرَتَيْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>مِنَّ الذّبِّ عَنْ أَعْرَاْضِهَا لَحَقِيِقُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مِنَّ</ar>
               </t>
               <t>
                  <ar>الذّبِّ</ar>
               </t>
               <t>
                  <ar>عَنْ</ar>
               </t>
               <t>
                  <ar>أَعْرَاْضِهَا</ar>
               </t>
               <t>
                  <ar>لَحَقِيِقُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨١٨١٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَاْمْتَاْحَ مِنّي حَلَبَاْتِ الْهَاْجِمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَاْمْتَاْحَ</ar>
               </t>
               <t>
                  <ar>مِنّي</ar>
               </t>
               <t>
                  <ar>حَلَبَاْتِ</ar>
               </t>
               <t>
                  <ar>الْهَاْجِمِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>شَأُوُ مُدِلٍّ سَاْبِقِ اللّهَاْمِمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>شَأُوُ</ar>
               </t>
               <t>
                  <ar>مُدِلٍّ</ar>
               </t>
               <t>
                  <ar>سَاْبِقِ</ar>
               </t>
               <t>
                  <ar>اللّهَاْمِمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٦٩٤٣٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَغَيْرُ سُفْعٍ مُثّلٍ يَحَاْمِمِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَغَيْرُ</ar>
               </t>
               <t>
                  <ar>سُفْعٍ</ar>
               </t>
               <t>
                  <ar>مُثّلٍ</ar>
               </t>
               <t>
                  <ar>يَحَاْمِمِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٨٥٠١٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>مَا أَقَلَّتْ قَدَمٌ نَاْعِلَهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>أَقَلَّتْ</ar>
               </t>
               <t>
                  <ar>قَدَمٌ</ar>
               </t>
               <t>
                  <ar>نَاْعِلَهَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>نِعِمَ السَّاْعُوْنَ فِي الْحَيِّ الشُّطُرْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>نِعِمَ</ar>
               </t>
               <t>
                  <ar>السَّاْعُوْنَ</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>الْحَيِّ</ar>
               </t>
               <t>
                  <ar>الشُّطُرْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٧٥١٥٨" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>ومَا كُّلَّ ذِي لُبٍّ بُمْؤْتِيْكَ نُصْحَهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ومَا</ar>
               </t>
               <t>
                  <ar>كُّلَّ</ar>
               </t>
               <t>
                  <ar>ذِي</ar>
               </t>
               <t>
                  <ar>لُبٍّ</ar>
               </t>
               <t>
                  <ar>بُمْؤْتِيْكَ</ar>
               </t>
               <t>
                  <ar>نُصْحَهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>ومَا كُّلُّ مُؤْتٍ نُصْحَهُ بَلَبِيْبِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ومَا</ar>
               </t>
               <t>
                  <ar>كُّلُّ</ar>
               </t>
               <t>
                  <ar>مُؤْتٍ</ar>
               </t>
               <t>
                  <ar>نُصْحَهُ</ar>
               </t>
               <t>
                  <ar>بَلَبِيْبِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
