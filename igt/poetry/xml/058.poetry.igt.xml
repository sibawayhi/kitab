<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="058">
   <verses id="٠٩٤٥٥" nbr="">
      <verse>
         <hemistiche>
            <text>
               <ar>فُكُوْنُوْا أَنْتُمْ وَبَنِي أَبِيْكُمْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فُكُوْنُوْا</ar>
                  <en/>
               </t>
               <t>
                  <ar>أَنْتُمْ</ar>
                  <en/>
               </t>
               <t>
                  <ar>وَبَنِي</ar>
                  <en/>
               </t>
               <t>
                  <ar>أَبِيْكُمْ</ar>
                  <en/>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>مَكَاْنَ الْكُلْيَتَيْنِ مِنَ الطِّحَاْلِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَكَاْنَ</ar>
                  <en/>
               </t>
               <t>
                  <ar>الْكُلْيَتَيْنِ</ar>
                  <en/>
               </t>
               <t>
                  <ar>مِنَ</ar>
                  <en/>
               </t>
               <t>
                  <ar>الطِّحَاْلِ</ar>
                  <en/>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٢٥١٩٥" nbr="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَكَاْنَ وَإِيَّاْهَا كَحَرّاْنَ لَمْ يُفِقْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَكَاْنَ</ar>
                  <en/>
               </t>
               <t>
                  <ar>وَإِيَّاْهَا</ar>
                  <en/>
               </t>
               <t>
                  <ar>كَحَرّاْنَ</ar>
                  <en/>
               </t>
               <t>
                  <ar>لَمْ</ar>
                  <en/>
               </t>
               <t>
                  <ar>يُفِقْ</ar>
                  <en/>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>عَنْ الْمَاْءِ إِذْ لَاقَاْهُ حَتَّى تَقَدَّدَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>عَنْ</ar>
                  <en/>
               </t>
               <t>
                  <ar>الْمَاْءِ</ar>
                  <en/>
               </t>
               <t>
                  <ar>إِذْ</ar>
                  <en/>
               </t>
               <t>
                  <ar>لَاقَاْهُ</ar>
                  <en/>
               </t>
               <t>
                  <ar>حَتَّى</ar>
                  <en/>
               </t>
               <t>
                  <ar>تَقَدَّدَا</ar>
                  <en/>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
