<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="477">
   <verses id="٠٠٥٤٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>ومَا حُلّ مِنْ جَهْلٍ حُبَى حُلَمَاْئِنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ومَا</ar>
               </t>
               <t>
                  <ar>حُلّ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>جَهْلٍ</ar>
               </t>
               <t>
                  <ar>حُبَى</ar>
               </t>
               <t>
                  <ar>حُلَمَاْئِنَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>ولَا قَاْئِلُ الْمَعْرُوْفِ فِيْنَا يُعَنَّفُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ولَا</ar>
               </t>
               <t>
                  <ar>قَاْئِلُ</ar>
               </t>
               <t>
                  <ar>الْمَعْرُوْفِ</ar>
               </t>
               <t>
                  <ar>فِيْنَا</ar>
               </t>
               <t>
                  <ar>يُعَنَّفُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
