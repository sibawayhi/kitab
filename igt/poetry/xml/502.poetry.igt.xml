<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="502">
   <verses id="٥٤٦١٥" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>وَطِرْتُ بُمَنْصُلِي فِي يَعْمَلَاْتٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>وَطِرْتُ</ar>
               </t>
               <t>
                  <ar>بُمَنْصُلِي</ar>
               </t>
               <t>
                  <ar>فِي</ar>
               </t>
               <t>
                  <ar>يَعْمَلَاْتٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>دَوَاْمِيُّ الْأَيْدِ يُخْبِطْنَ السَّرِيْحَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>دَوَاْمِيُّ</ar>
               </t>
               <t>
                  <ar>الْأَيْدِ</ar>
               </t>
               <t>
                  <ar>يُخْبِطْنَ</ar>
               </t>
               <t>
                  <ar>السَّرِيْحَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
