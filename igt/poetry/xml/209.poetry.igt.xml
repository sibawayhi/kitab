   <verses id="٣٤٩٧٩" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>إِلَيْكَ حَتَّى بَلَغَتْ إيَّاْكَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِلَيْكَ</ar>
               </t>
               <t>
                  <ar>حَتَّى</ar>
               </t>
               <t>
                  <ar>بَلَغَتْ</ar>
               </t>
               <t>
                  <ar>إيَّاْكَا</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٥٧٢٣٧" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>كَأَنَّا يَوْمَ قُرَّى إِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَأَنَّا</ar>
               </t>
               <t>
                  <ar>يَوْمَ</ar>
               </t>
               <t>
                  <ar>قُرَّى</ar>
               </t>
               <t>
                  <ar>إِ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>نَّمَا نَقْتُلُ إِيَّاْنَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>نَّمَا</ar>
               </t>
