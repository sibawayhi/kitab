<?xml version="1.0" encoding="utf-8"?>
<!---->
<poetry xmlns="http://www.sibawayhi.org/schema/2024/05/igt" bab="197">
   <verses id="٠٣٣٧٤" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>فَمَا لِىَ إِلَّاّْ اللهُ لَا رَبَّ غَيَرِهُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>فَمَا</ar>
               </t>
               <t>
                  <ar>لِىَ</ar>
               </t>
               <t>
                  <ar>إِلَّاّْ</ar>
               </t>
               <t>
                  <ar>اللهُ</ar>
               </t>
               <t>
                  <ar>لَا</ar>
               </t>
               <t>
                  <ar>رَبَّ</ar>
               </t>
               <t>
                  <ar>غَيَرِهُ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>ومَا لِىَ إِلَّاّْ اللهَ غَيَرَكَ نَاْصِرُ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>ومَا</ar>
               </t>
               <t>
                  <ar>لِىَ</ar>
               </t>
               <t>
                  <ar>إِلَّاّْ</ar>
               </t>
               <t>
                  <ar>اللهَ</ar>
               </t>
               <t>
                  <ar>غَيَرَكَ</ar>
               </t>
               <t>
                  <ar>نَاْصِرُ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩٤٦٦٠" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>يًّا كَعْبُ صَبْرًا عَلَى مَا كَاْنَ مِنْ حَدَثٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يًّا</ar>
               </t>
               <t>
                  <ar>كَعْبُ</ar>
               </t>
               <t>
                  <ar>صَبْرًا</ar>
               </t>
               <t>
                  <ar>عَلَى</ar>
               </t>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>كَاْنَ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>حَدَثٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>يَا كَعْبُ لَمْ يَبْقَ مِنّا غَيْرُ أَجْلَاْدِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>يَا</ar>
               </t>
               <t>
                  <ar>كَعْبُ</ar>
               </t>
               <t>
                  <ar>لَمْ</ar>
               </t>
               <t>
                  <ar>يَبْقَ</ar>
               </t>
               <t>
                  <ar>مِنّا</ar>
               </t>
               <t>
                  <ar>غَيْرُ</ar>
               </t>
               <t>
                  <ar>أَجْلَاْدِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
      <verse>
         <hemistiche>
            <text>
               <ar>إَلَاَّْ بَقِيّاْتُ أَنْفَاْسٍ تُحَشْرِجُهَا</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إَلَاَّْ</ar>
               </t>
               <t>
                  <ar>بَقِيّاْتُ</ar>
               </t>
               <t>
                  <ar>أَنْفَاْسٍ</ar>
               </t>
               <t>
                  <ar>تُحَشْرِجُهَا</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>كَرَاْحِلٍ رَاْئِحٍ أَوْ بَاْكِرٍ غَاْدِى</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>كَرَاْحِلٍ</ar>
               </t>
               <t>
                  <ar>رَاْئِحٍ</ar>
               </t>
               <t>
                  <ar>أَوْ</ar>
               </t>
               <t>
                  <ar>بَاْكِرٍ</ar>
               </t>
               <t>
                  <ar>غَاْدِى</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٤٧٨٨٦" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>مَا بِاْلْمَدِيْنَةِ دَاْرٌ غَيْرُ وَاحِدَةٍ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَا</ar>
               </t>
               <t>
                  <ar>بِاْلْمَدِيْنَةِ</ar>
               </t>
               <t>
                  <ar>دَاْرٌ</ar>
               </t>
               <t>
                  <ar>غَيْرُ</ar>
               </t>
               <t>
                  <ar>وَاحِدَةٍ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>دَاْرُ الْخَلِيْفَةِ إِلَّاّْ دَاْرُ مَرْوَاْنِ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>دَاْرُ</ar>
               </t>
               <t>
                  <ar>الْخَلِيْفَةِ</ar>
               </t>
               <t>
                  <ar>إِلَّاّْ</ar>
               </t>
               <t>
                  <ar>دَاْرُ</ar>
               </t>
               <t>
                  <ar>مَرْوَاْنِ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
   <verses id="٩٣٩٣١" poet="">
      <verse>
         <hemistiche>
            <text>
               <ar>مَاْلِكَ مِنْ شَيْخِكَ إِلَّاّْ عَمَلُهْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>مَاْلِكَ</ar>
               </t>
               <t>
                  <ar>مِنْ</ar>
               </t>
               <t>
                  <ar>شَيْخِكَ</ar>
               </t>
               <t>
                  <ar>إِلَّاّْ</ar>
               </t>
               <t>
                  <ar>عَمَلُهْ</ar>
               </t>
            </igt>
         </hemistiche>
         <hemistiche>
            <text>
               <ar>إِلَاّْ رَسِيْمَهُ وَإِلَّاّْ رَمَلُهْ</ar>
               <free/>
            </text>
            <igt>
               <t>
                  <ar>إِلَاّْ</ar>
               </t>
               <t>
                  <ar>رَسِيْمَهُ</ar>
               </t>
               <t>
                  <ar>وَإِلَّاّْ</ar>
               </t>
               <t>
                  <ar>رَمَلُهْ</ar>
               </t>
            </igt>
         </hemistiche>
      </verse>
   </verses>
</poetry>
