     البكاء="١-٣٥٥"
     هارون="١-٢٧٣"
     يعقوب="١-٣٣٠"
     رقم="٥٣">

  <عنوان>
    <مقطع هوية='٣٧٦٢٣'>هذا باْبُ ما يَنْتَصِبُ على إِضْماْرِ الْفِعْلِ الْمَتْرُوْكِ إِظْهاْرُهُ اسْتِغْناْءً عَنْهُ</مقطع>
  </عنوان>

  <مقطع هوية='٥٩١٧٧'>وَسَأُمَثِّلُهُ لَكَ مُظْهَرًا لِتَعْلَمَ ما أَراْدُوْا إِنْ شَاْءَ اللّٰهُ تَعَاْلَى</مقطع>

  <التحرير>
    <تشكيل  نسخة="٠" نسبة="١٠٠"/>
    <تقطيع نسخة="١" نسبة="١٠٠"/>
    <أقوال  نسخة="٠" نسبة="٠"/>
    <شواهد  نسخة="٠" نسبة="٠"/>
    <آيات  نسخة="٠" نسبة="٠"/>
  </التحرير>

</باب>
