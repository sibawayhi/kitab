<?xml version="1.0" encoding="utf-8"?>

<باب xmlns="http://www.sibawayhi.org/schema/2009/07/sibawayhi"
     بلاق="١-٣٨٢"
     درنبورج="١-٣٣٥"
     البكاء="٤-٢٧"
     هارون="٢-٣٦٢"
     يعقوب="٢-٣٨٤"
     رقم="٢٠٩">

  <عنوان>
    <مقطع هوية='١٦٩٢٧'>هُذَا بَاْبُ مَا يَجُوْزُ فِي الشِّعْرِ مِنْ إيَّا وَلَا يَجُوْزُ فِي الْكَلَاْمِ</مقطع>
  </عنوان>
  <مقطع هوية='١١١٦٤'>فَمِنْ ذٰلِكَ قَوْلُ حُمَيْدٍ الْأَرْقَطِ</مقطع>

  <شاهدة هوية='٣٤٩٧٩'>
    <بيت>
      <مصراع>إِلَيْكَ حَتَّى بَلَغَتْ إيَّاْكَا</مصراع>
    </بيت>
  </شاهدة>

  <مقطع هوية='١٠٦٢١'>وَقَاْلَ الْآخَرُ لِبَعْضِ اللَّصَوْصِ</مقطع>

  <شاهدة هوية='٥٧٢٣٧'>
    <بيت>
      <مصراع>كَأَنَّا يَوْمَ قُرَّى إ</مصراع>
      <مصراع>نَّمَا نَقْتُلُ إيَّاْنَا</مصراع>
    </بيت>
    <بيت>
      <مصراع>[ قُتِلْنْا مِنْهُمْ كَلَّ</مصراع>
      <مصراع>فَتًى أَبْيَضَ حُسَّاْنَاَْ</مصراع>
    </بيت>
  </شاهدة>

  <التحرير>
    <تشكيل  نسخة="٠" نسبة="١٠٠"/>
    <تقطيع نسخة="٠" نسبة="٠"/>
    <أقوال  نسخة="٠" نسبة="٠"/>
    <شواهد  نسخة="٠" نسبة="٠"/>
  </التحرير>

</باب>
