<باب xmlns="http://www.sibawayhi.org/schema/2009/07/sibawayhi"
     بلاق="١-٣٧٧"
     درنبورج="١-٣٢٩"
     البكاء="٤-٩"
     هارون="٢-٣٥٠"
     يعقوب="٢-٣٦٩"
     رقم="٢٠٣">

   <عنوان>
     <مقطع هوية='٥٠٠٨٠'>هٰذَا بَاْبُ مَجْرَى عَلَاْمَاْتِ الْمُضْمَرِيْنَ</مقطع>
     <مقطع هوية='٨٤٤٤٢'>وَمَا يَجُوْزُ فِيْهِنَّ كُلِّهِنَّ</مقطع>
   </عنوان>
   <مقطع هوية='٧٠٣٧٩'>وَسَنُيِّنُ ذٰلِكَ إنْ شَاْءَ اللهُ</مقطع>

   <التحرير>
     <تشكيل  نسخة="١٠٠" نسبة="١٠٠"/>
     <تقطيع نسخة="١٠٠" نسبة="١٠٠"/>
   </التحرير>

</باب>
