<?xml version="1.0" encoding="utf-8"?>

<باب xmlns="http://www.sibawayhi.org/schema/2009/07/sibawayhi"
     بلاق="١-١٨٣"
     درنبورج="١-١٥٤"
     البكاء="١-٤٦٥"
     هارون="١-٣٦٦"
     يعقوب="١-٤٣٣"
     رقم="٧٩">

    <عنوان>
       <مقطع هوية='٣٣٥٧٤'>هٰذَا بَاْبٌ لَا يَكُوْنُ فِيْهِ إِلَّا الرَّفْعُ</مقطع>
   </عنوان>
    <مقطع هوية='٢١٩٢١'>وَذٰلِكَ قَوْلُكَ <قول هوية='٠٦٥٢٢'>لَهُ يَدٌ يَدُ الثَّوْرِ</قول>
   </مقطع>
    <مقطع هوية='٤١٠٧٠'>وَ<قول هوية='٧٨٧٣٠'>لَهُ رَأْسٌ رَأْسُ الْحِمَاْرِ</قول>
   </مقطع>
    <مقطع هوية='٦٠٧٥٠'>لِأَنَّ  <حرف>هٰذَا</حرف> اسْمٌ</مقطع>
    <مقطع هوية='١٨٢٠٦'>وَلَا يُوَهَّمُ عَلَى الرَّجُلِ أَنَّهُ يَصْنَعُ يَدًا وَلَا رِجْلًا</مقطع>
    <مقطع هوية='٧١٣٦٨'>وَلَيْسَ بِفِعْلٍ</مقطع>

    <التحرير>
      <تشكيل  نسخة="٠" نسبة="١٠٠"/>
      <تقطيع نسخة="١" نسبة="١٠٠"/>
      <أقوال  نسخة="٠" نسبة="٠"/>
    </التحرير>

</باب>
