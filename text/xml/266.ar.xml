<باب xmlns="http://www.sibawayhi.org/schema/2009/07/sibawayhi"
     بلاق="١-٤٦٦"
     درنبورج="١-٤١٥"
     البكاء="٤-٢٦٦"
     هارون="٣-١٣٢"
     يعقوب="٣-١٥٢"
     رقم="٢٦٦">

  <عنوان>
    <مقطع هوية='٤٢٣٤٧'>هٰذَا بَاْبٌ تَكُوْنُ فِيْهِ <حرف>أَنَّ</حرف> بَدَلًا مِنْ شَيْءٍ هُوَ الْأَوْلُ</مقطع>
  </عنوان>
  <مقطع هوية='٩٠٤٩٤'>وَذٰلِكَ قَوْلُك بَلَغَنِي قِصَّتُكَ أَنَّكَ فَاْعِلٌ وَقَدْ بَلَغَنِي الْحَدِيْثُ أَنَّهُمْ مُنْطَلِقُوْنَ وَكَذٰلِكَ الْقِصَّةُ وَمَا أَشْبَهْهَا</مقطع>

  <التحرير>
    <تشكيل  نسخة="٠" نسبة="١٠٠"/>
    <تقطيع نسخة="٠" نسبة="٠"/>
    <أقوال  نسخة="٠" نسبة="٠"/>
    <شواهد  نسخة="٠" نسبة="٠"/>
  </التحرير>

</باب>
