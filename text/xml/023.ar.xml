<باب xmlns="http://www.sibawayhi.org/schema/2009/07/sibawayhi"
     بلاق="١-٣٧"
     درنبورج="١-٢٨"
     البكاء="١-١٤٠"
     هارون="١-٧٣"
     يعقوب="١-١٢٣"
     رقم="٢٣">
  <عنوان>
    <مقطع هوية='٣٧٠٣٨' رقم="١">هٰذَا بَاْبُ الْفَاْعِلَيْنِ وَالْمَفْعُوْلَيْنِ اللَّذَيْنِ كَلُّ وَاْحِدٍ مِنْهُمَا يَفْعَلُ بِفَاْعِلِهِ مِثْلَ الَّذِي يَفْعَلُ بِهِ</مقطع>
    <مقطع هوية='١٥٩٨٥' رقم="٢">وَمَا كَاْنَ نَحْوَ ذٰلِكَ</مقطع>
  </عنوان>
  <مقطع هوية='٣٢٠٤٨' رقم="٣">وَهُوَ قَوْلُكَ <قول هوية='٧٢٩٩٥' رقم="١">ضَرَبْتُ وَضَرَبَنِي زَيْدٌ</قول>
  </مقطع>
  <مقطع هوية='٢٩٠٨٩' رقم="٤">وَ<قول هوية='٩٤٤٢٣' رقم="٢">ضَرَبَنِي وَضَرَبْتُ زَيْدًا</قول>
  </مقطع>
  <مقطع هوية='٧٥٥٦٨' رقم="٥">تَحْمِلُ الِاسْمَ عَلَى الْفِعْلِ الَّذِي يَلِيْهِ</مقطع>
  <مقطع هوية='٨٢٤٠٢' رقم="٦">فَالْعَاْمِلُ فِي اللَّفْظِ أَحَدُ الْفِعْلَيْنِ</مقطع>
  <مقطع هوية='٢٥٥٩٥' رقم="٧">وَأَمَّا فِي الْمَعْنَى</مقطع>
  <مقطع هوية='٩٧٤٨٦' رقم="٨">فَقَدْ يُعْلَمُ أَنَّ الْأَوَّلَ قَدْ وَقَعَ</مقطع>
  <مقطع هوية='٦٨٧٧٠' رقم="٩">إِلَّا أَنَّهُ لَا يُعْمَلُ فِي اسْمٍ وَاْحِدٍ نَصْبٌ وَرَفْعٌ</مقطع>
  <مقطع هوية='٣٢٣٢٩' رقم="١٠">وَإِنَّمَا كَاْنَ الَّذِي يَلِيْهِ أَوْلَى لِقُرْبِ جِوَاْرِهِ</مقطع>
  <مقطع هوية='٧٩٨٧٤' رقم="١١">وَأَنَّهُ لَا يَنْقُضُ مَعْنًى</مقطع>
  <مقطع هوية='٤٩١٩٩' رقم="١٢">وَأَنَّ الْمُخَاْطَبَ قَدْ عَرَفَ أَنَّ الْأَوَّلَ قَدْ وَقَعَ بِزَيْدٍ</مقطع>
  <مقطع هوية='٤٧٢٠٠' رقم="١٣">كَمَا كَاْنَ <قول هوية='٢٦٣٢٧' رقم="٣">خَشَّنْتُ بِصَدْرِهِ وَصَدْرِ زَيْدٍ</قول> وَجْهَ الْكَلَامِ</مقطع>
  <مقطع هوية='٠٠٨٩٥' رقم="١٤">حَيْثُ كَاْنَ الْجَرُّ فِي الْأَوَّلِ</مقطع>
  <مقطع هوية='٧١٣٣٧' رقم="١٥">وَكَاْنَتِ الْبَاْءُ أَقَرَبَ إِلَى الِاسْمِ مِنْ الْفِعْلِ وَلَا تَنْقُضْ مَعْنًى</مقطع>
  <مقطع هوية='٤٥٠٠٤' رقم="١٦">سَوَّوْا بَيْنَهُمَا فِي الْجَرِّ كَمَا يَسْتَوِيَاْنِ فِي النَّصْبِ</مقطع>
  <مقطع هوية='٤٩٥٣١' رقم="١٧">وَمِمَّا يُقَوِّي تَرْكَ نَحْوِ هٰذَا لِعِلْمِ الْمُخَاْطَبِ</مقطع>
  <مقطع هوية='٦٤١٧٩' رقم="١٨">قَوْلُهُ عَزَّ وَجَلَّ <آية هوية='٩١٧٦٣' سورة="٣٣" آية="٣٥">وَالْحَاْفِظِيْنَ فُرُوْجَهُمْ وَالْحَاْفِظَاْتِ وَالذَّاْكِرِيْنَ اللّٰهَ كَثِيْرًا وَالذَّاْكِرَاْتِ</آية>
  </مقطع>
  <مقطع هوية='٠١٤٨٥' رقم="١٩">فَلَمْ يُعْمِلُ الْآخِرَ فِيْمَا عَمِلَ فِيْهِ الْأَوَّلُ اسْتِغْنَاْءً عَنْهُ</مقطع>
  <مقطع هوية='٨٩٢١٢' رقم="٢٠">وَمِثْلُ ذٰلِكَ <قول هوية='٩٤٨٦٩' رقم="٤">وَنَخْلَعُ وَنَتْرُكُ مَنْ يَفْجُرُكَ</قول>
  </مقطع>
  <مقطع هوية='٧٢٠٩٢' رقم="٢١">وَجَاْءَ فِي الشِّعْرِ مِنَ الِاسْتِغْنَاْءِ أَشَدُّ مِنِ هٰذَا</مقطع>
  <مقطع هوية='٩٨٧٢٧' رقم="٢٢">وَذٰلِكَ قَوْلُ قَيْسِ بْنِ الْخَطِيْمِ</مقطع>
  <شاهدة هوية='٦٨٠٤٣' مقطع="٢٣" رقم="١">
    <بيت>
      <مصراع>نَحْنُ بِمَا عِنْدَنَا وَأَنْتَ بِمَا</مصراع>
      <مصراع>عِنْدَكَ رَاْضٍ وَالرَّأْيُ مُخْتَلِفُ</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٢٧٠٩٨' رقم="٢٤">وَقَاْلَ ضَاْبِيءٌ البُرْجُمِيُّ</مقطع>
  <شاهدة هوية='٩٥٦١٧' مقطع="٢٥" رقم="٢">
    <بيت>
      <مصراع>فَمَنْ يَكُ أَمْسَى بِالْمَدِيْنَةِ رَحْلُهُ</مصراع>
      <مصراع>فَإِنِّي وَقَيَّاْرًا بِهَا لَغَرِيْبُ</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٥٦١٨٦' رقم="٢٦">وَقَاْلَ ابْنُ أَحْمَرَ</مقطع>
  <شاهدة هوية='٦٢٢٨٢' مقطع="٢٧" رقم="٣">
    <بيت>
      <مصراع>رَمَاْنِي بِأَمْرٍ كُنْتُ مِنْهُ وَوَاْلِدِي</مصراع>
      <مصراع>بَرِيْئًا وَمِنْ أَجْلِ الطَّوِيِّ رَمَاْنِي</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٦٦٢٥٣' رقم="٢٨">فَوَضَعَ فِي مَوْضِعِ الْخَبَرِ لَفْظَ الْوَاْحِدِ</مقطع>
  <مقطع هوية='٣٠٧٥٦' رقم="٢٩">لِأَنَّهُ قَدْ عَلِمَ أَنَّ الْمُخَاْطَبَ سَيَسْتَدَّلُ بِهِ</مقطع>
  <مقطع هوية='١٠٥٥٢' رقم="٣٠">عَلَى أَنَّ الْآخَرِيْنَ فِي هَذِهِ الصِّفَةِ</مقطع>
  <مقطع هوية='٤٩٢٦٢' رقم="٣١">وَالْأَوَّلُ أَجْوَدُ لِأَنَّهُ لَمْ يَضَعْ وَاْحِدًا فِي مَوْضِعِ جَمْعٍ</مقطع>
  <مقطع هوية='٢٥٦٥٣' رقم="٣٢">وَلَا جَمْعًا فِي مَوْضِعِ وَاْحِدٍ</مقطع>
  <مقطع هوية='٥٥١٧٧' رقم="٣٣">وَمِثْلُهُ قَوْلُ الْفَرَزْدَقِ</مقطع>
  <شاهدة هوية='٦٤٠٠٣' مقطع="٣٤" رقم="٤">
    <بيت>
      <مصراع>إِنِّي ضَمِنْتُ لِمَنْ أَتَاْنِي مَا جَنَى</مصراع>
      <مصراع>وَأَبِي فَكَاْنَ وَكُنْتُ غَيْرَ غَدُوْرِ</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٠٣٠٢٢' رقم="٣٥">تَرَكَ أَنْ يَكُوْنَ لِلْأَوَّلِ خَبَرٌ اِسْتِغْنَاْءً <!-- [حِيْنَ اسْتَغْنَى] -->بِالْآخِرِ</مقطع>
  <مقطع هوية='٨٠٥٣٤' رقم="٣٦">وَلِعِلْمِ الْمُخَاْطَبِ أَنَّ الْأَوَّلَ قَدْ دَخَلَ فِي ذٰلِكَ</مقطع>
  <مقطع هوية='٢٧١٦٩' رقم="٣٧">وَلَوْ لَمْ تَحْمِلْ الْكَلَاْمَ عَلَى الْآخِرِ</مقطع>
  <مقطع هوية='٩١٦١٠' رقم="٣٨">لَقُلْتَ <قول هوية='٨٤٣٣٣' رقم="٥">ضَرَبْتُ وَضَرَبُوْنِي قَوْمَكَ</قول>
  </مقطع>
  <مقطع هوية='٣٤٩٨٩' رقم="٣٩">وَإِنَّمَا كَلَاْمُهُمْ <قول هوية='٧٣٦٣١' رقم="٦">ضَرَبْتُ وَضَرَبَنِي قَوْمُكَ</قول>
  </مقطع>
  <مقطع هوية='٣٦٢٧٨' رقم="٤٠">وَإِذَا قُلْتَ <حرف>ضَرَبَنِي</حرف> لَمْ يَكُنْ سَبِيْلٌ لِلأَوَّلِ</مقطع>
  <مقطع هوية='٥٤٧٨٧' رقم="٤١">لِأَنَّكَ لَا تَقُوْلُ <حرف>ضَرَبَنِي</حرف> وَأَنْتَ تَجَعْلُ الْمُضْمَرُ جَمِيْعًا</مقطع>
  <مقطع هوية='٠٢٤٠٥' رقم="٤٢">وَلَوْ أَعْمَلْتَ الْأَوَّلَ لَقُلْتَ <قول هوية='١٠٣٣٥' رقم="٧" ج="*">مَرَرْتُ وَمَرَّ بِي بِزَيْدٍ</قول>
  </مقطع>
  <مقطع هوية='٦٢٥٢٠' رقم="٤٣">وَإِنَّمَا قَبُحَ هٰذَا أَنَّهُمْ قَدْ جَعَلُوْا الْأَقْرَبَ أَوْلَى إِذَا لَمْ يَنْقُضْ مَعْنًى</مقطع>
  <مقطع هوية='٥٦٥٢١' رقم="٤٤">قَاْلَ الشَّاْعِرُ وَهُوَ الْفَرَزْدَقُ</مقطع>
  <شاهدة هوية='١٢٩٨٩' مقطع="٤٥" رقم="٥">
    <بيت>
      <مصراع>وَلَكِنَّ نِصْفًا لَوْ سَبَبْتُ وسَبَّنِي</مصراع>
      <مصراع>بَنُو عَبْدِ شَمْسٍ مِنْ مَنَاْفٍ وَهَاْشِمِ</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٩٩٢٩٩' رقم="٤٦">وَقَاْلَ طُفَيْلٌ الْغَنَوِيُّ</مقطع>
  <شاهدة هوية='٨٤٠٠٣' مقطع="٤٧" رقم="٦">
    <بيت>
      <مصراع>وَكُمْتًا مُدَمَّاْةً كَأَنَّ مُتُوْنَهَا</مصراع>
      <مصراع>جَرَى فَوْقَهَا وَاسْتَشْعَرَتْ لَوْنَ مُذْهَبِ</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٩١٤٦٧' رقم="٤٨">وَقَاْلَ رَجُلٌ مِنْ بَاْهِلَةَ</مقطع>
  <شاهدة هوية='٧٥٦٠٢' مقطع="٤٩" رقم="٧">
    <بيت>
      <مصراع>وَلَقَدْ أَرَى تَغْنَى بِهِ سَيْفَاْنَةٌ</مصراع>
      <مصراع>تُصْبِي الْحَلِيْمَ وَمِثْلُهَا أَصْبَاْهُ</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٠٧١٢٨' رقم="٥٠">فَالْفِعْلُ الْأَوَّلُ فِي كُلِّ هٰذَا مُعْمَلٌ فِي الْمَعْنَى وَغَيْرُ مُعْمَلٍ فِي اللَّفْظِ</مقطع>
  <مقطع هوية='٤٠٢٦٦' رقم="٥١">وَالْآخِرُ مُعْمَلٌ فِي اللَّفْظِ وَالْمَعْنَى</مقطع>
  <مقطع هوية='٥٤٩٨٦' رقم="٥٢">فَإِنْ قُلْتَ <قول هوية='٣٢١٠٧' رقم="٨">ضَرَبْتُ وَضَرَبُوْنِي قَوْمَكَ</قول> نَصَبْتَ</مقطع>
  <مقطع هوية='١٣٣٥٦' رقم="٥٣">إِلَّا فِي قَوْلِ مَنْ قَاْلَ <قول هوية='٧٠٩٩٧' رقم="٩">أَكَلُوْنِي الْبَرَاْغِيْثُ</قول>
  </مقطع>
  <مقطع هوية='٨٦٥٢٤' رقم="٥٤">أَوْ تَحْمِلُهُ عَلَى الْبَدَلِ فَتَجْعَلُهُ بَدَلًا مِنَ الْمُضْمَرِ</مقطع>
  <مقطع هوية='٢٧٢٥٢' رقم="٥٥">كَأَنَّكَ قُلْتَ <قول هوية='٠١٠٦٨' رقم="١٠">ضَرَبْتُ وَضَرَبَنِي نَاْسٌ بَنُو فُلَاْنٍ</قول>
  </مقطع>
  <مقطع هوية='٧٤٢٦٧' رقم="٥٦">وَعَلَى هٰذَا الْحَدِّ تَقُوْلُ <قول هوية='٢٤٣٢٦' رقم="١١">ضَرَبْتُ وَضَرَبَنِي عَبْدَ اللّٰهِ</قول>
  </مقطع>
  <مقطع هوية='٢٤٤٣٨' رقم="٥٧">تُضْمِرُ في <حرف>ضَرَبَنِي</حرف> كَمَا أَضْمَرْتَ في <حرف>ضَرَبُوْنِي</حرف>
  </مقطع>
  <مقطع هوية='٨٧٦٢٩' رقم="٥٨">فَإِنْ قُلْتَ <قول هوية='٥٤٨٠٤' رقم="١٢">ضَرَبَنِي وَضَرَبْتُهُمْ قَوْمُكَ</قول>
  </مقطع>
  <مقطع هوية='٠٣٧١٤' رقم="٥٩">رَفَعْتَ لِأَنَّكَ شَغَلْتَ الْآخِرَ فَأَضْمَرْتَ فِيْهِ</مقطع>
  <مقطع هوية='٨٠٩٥٢' رقم="٦٠">كَأَنَّكَ قُلْتَ <قول هوية='٠٦٩٨٢' رقم="١٣">ضَرَبَنِي قَوْمُكَ وَضَرَبْتَهُمْ</قول>
  </مقطع>
  <مقطع هوية='٢٨٨٢٩' رقم="٦١">عَلَى التَّقْدِيْمِ وَالتَّأْخِيْرِ</مقطع>
  <مقطع هوية='٨٣٣٦٧' رقم="٦٢">إِلَّا أَنْ تَجْعَلَ هٰهُنَا الْبَدَلَ كَمَا جَعَلْتَهُ في الرَّفْعِ</مقطع>
  <مقطع هوية='١٢٦٣٠' رقم="٦٣">فَإِنْ فَعَلْتَ ذٰلِكَ لَمْ يَكُنْ بُدٌّ مِنْ <حرف>ضَرَبُوْنِي</حرف>
  </مقطع>
  <مقطع هوية='٦٢٢٣٦' رقم="٦٤">لِأَنَّكَ تُضْمِرُ فِيْهِ الْجَمْعَ</مقطع>
  <مقطع هوية='١٦٤٩٥' رقم="٦٥">قَالَ عُمَرُ بْنُ أَبِي رَبِيْعَةَ</مقطع>
  <شاهدة هوية='٤٤٤١٧' مقطع="٦٦" رقم="٨">
    <بيت>
      <مصراع>إِذَا هِىَ لَمْ تَسْتَكْ بِعُوْدِ أَرَاكَةٍ</مصراع>
      <مصراع>تُنُخِّلَ فَاسْتَاكَتْ بِهِ عُوْدُ إِسْحِلِ</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٤٩٥٣٧' رقم="٦٧">لِأَنَّهُ أَضْمَرَ في آخِرِ الْكَلَامِ</مقطع>
  <مقطع هوية='٠٢٧٨٩' رقم="٦٨">وَقَالَ المَرَّارُ الَأَسَدِيُّ</مقطع>
  <شاهدة هوية='٥٨٣١٥' مقطع="٦٩" رقم="٩">
    <بيت>
      <مصراع>فَرَدَّ عَلَى الْفُؤَادِ هَوًى عَمِيْدًا</مصراع>
      <مصراع>وَسُوْئِلَ لَوْ يُبِيْنُ لَنَا سُؤَالْا</مصراع>
    </بيت>
    <بيت>
      <مصراع>وَقَدْ نَغْنَى بِهَا وَنَرَى عُصُوْرا</مصراع>
      <مصراع>بِهَا يَقْتَدْنَنَا الْخُرُدَ الْخِدَالا</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٧٦١٢١' رقم="٧٠">حَدَّثَنَا بِهِ أَبُوْ الْخَطَّابِ عَنْ شَاعِرِهِ</مقطع>
  <مقطع هوية='٦٢١٠٧' رقم="٧١">وَإِذَا قُلْتَ <قول هوية='٣٩٧١٤' رقم="١٤">ضَرَبُوْنِي وَضَرَبْتُهُمْ قَوْمَكَ</قول>
  </مقطع>
  <مقطع هوية='٦٥٦٢٨' رقم="٧٢">جَعَلْتَ الْـ<حرف>قَوْمَ</حرف> بَدَلَا مِنْ <حرف>هُمْ</حرف>
  </مقطع>
  <مقطع هوية='٩٩٢٤٧' رقم="٧٣">لِأَنَّ الْفِعْلَ لَا بُدَّ لَهُ مِنْ فَاْعِلٍ</مقطع>
  <مقطع هوية='٨١٤٨٢' رقم="٧٤">وَالْفَاْعِلُ هٰهُنَا جَمَاْعَةٌ وَضَمِيْرُ الْجَمَاْعَةِ الْوَاْوُ</مقطع>
  <مقطع هوية='٢٢٧٩٣' رقم="٧٥">وَكَذٰلِكَ تَقُوْلُ <قول هوية='٣٨٤٧٨' رقم="١٥">ضَرَبُوْنِي وَضَرَبْتُ قَوْمَكَ</قول>
  </مقطع>
  <مقطع هوية='٦٣٢٥٦' رقم="٧٦">إِذَا أَعْمَلْتَ الْآخِرَ</مقطع>
  <مقطع هوية='٤٧٨٣٩' رقم="٧٧">فَلَا بُدَّ في الْأَوَّلِ مِنْ ضَمِيْرِ الْفَاْعِلِ لِئَلَّا يَخْلُوَ مِنْ فَاْعِلٍ</مقطع>
  <مقطع هوية='٢١٩٠٥' رقم="٧٨">وَإِنَّمَا قُلْتَ <قول هوية='٤١٢٢٤' رقم="١٦">ضَرَبْتُ وَضَرَبَنِي قَوْمُكَ</قول>
  </مقطع>
  <مقطع هوية='٥٤٢٢٣' رقم="٧٩">فَلَمْ تَجْعَلْ فِي الْأَوَّلِ الْهَاْءَ وَالْمِيْمَ</مقطع>
  <مقطع هوية='٧٣٢٥٨' رقم="٨٠">لِأَنَّ الْفِعْلَ قَدْ يَكُوْنُ بِغَيْرِ مَفْعُوْلٍ</مقطع>
  <مقطع هوية='٤١٢٧٩' رقم="٨١">وَلَا يَكُوْنُ الْفِعْلُ بِغَيْرِ فَاْعِلٍ</مقطع>
  <مقطع هوية='٢٧١٧٧' رقم="٨٢">وَقَاْلَ امْرُؤُ الْقَيْسِ</مقطع>
  <شاهدة هوية='٧٢٠٠٤' مقطع="٨٣" رقم="١٠">
    <بيت>
      <مصراع>فَلَوْ أَنَّ مَا أَسْعَى لِأَدْنَى مَعِيْشَةٍ</مصراع>
      <مصراع>
      <ثبات>كَفَاْنِي</ثبات> وَلَمْ أَطْلُبْ <ثبات>قَلِيْلٌ</ثبات> مِنَ الْمَاْلِ</مصراع>
    </بيت>
  </شاهدة>
  <مقطع هوية='٥٥١٤٠' رقم="٨٤">فَإِنَّمَا رَفَعَ لِأَنَّهُ لَمْ يَجْعَلْ الْـ<حرف>قَلِيْلَ</حرف> مَطْلُوْبًا</مقطع>
  <مقطع هوية='٥٥٥٨٢' رقم="٨٥">وَإِنَّمَا كَانَ الْمَطْلُوْبُ عِنْدَهَ الْمُلْكَ</مقطع>
  <مقطع هوية='٢٦٠٩٣' رقم="٨٦">وَجَعَلَ الْـ<حرف>قَلِيْلَ</حرف> كَاْفِيًا</مقطع>
  <مقطع هوية='٤٣٧٨٧' رقم="٨٧">وَلَوْ لَمْ يُرِدْ ذٰلِكَ وَنَصَبَ فَسَدَ الْمَعْنَى</مقطع>
  <مقطع هوية='٢٢٨٢٣' رقم="٨٨">وَقَدْ يَجُوْزُ <قول هوية='٧٤٦٠١' رقم="١٧">ضَرَبْتُ وَضَرَبَنِي زَيْدًا</قول>
  </مقطع>
  <مقطع هوية='٩١٩٦٨' رقم="٨٩">لِأَنَّ بَعْضَهُمْ قَدْ يَقُوْلُ <قول هوية='٢٣٥٩٣' رقم="١٨">مَتَى رَأَيْتَ أَوْ قُلْتَ زَيْدًا مُنْطَلِقًا</قول>
  </مقطع>
  <مقطع هوية='٠٧٥٣٥' رقم="٩٠">وَالْوَجْهُ <قول هوية='٧٣٠٦٤' رقم="١٩">مَتَى رَأَيْتَ أَوْ قُلْتَ زَيْدٌ مُنْطَلِقٌ</قول>
  </مقطع>
  <مقطع هوية='٥٥٩٨٠' رقم="٩١">وَمِثْلُ ذٰلِكَ في الْجَوَاْزِ <قول هوية='٨٧١٦٠' رقم="٢٠">ضَرَبَنِي وَضَرَبْتُ قَوْمُكَ</قول>
  </مقطع>
  <مقطع هوية='٩٢٦٦٥' رقم="٩٢">وَالْوَجْهُ أَنْ تَقُوْلَ <قول هوية='٩٦٨٧٨' رقم="٢١">ضَرَبُوْنِي وَضَرَبْتُ قَوْمَكَ</قول></مقطع>
  <مقطع هوية='٥٥٨٣٦' رقم="٩٣">فَتَحْمِلَهُ عَلَى الْآخِرِ</مقطع>
  <مقطع هوية='٣٣٠٥١' رقم="٩٤">فَإِنْ قُلْتَ <قول هوية='١٢٥١٣' رقم="٢٢">ضَرَبَنِي وَضَرَبْتُ قَوْمَكَ</قول></مقطع>
  <مقطع هوية='٥٦٠٧٣' رقم="٩٥">فَجَاْئِزٌ وَهُوَ قَبِيْحٌ أَنْ تَجْعَلَ اللَّفْظَ كَالْوَاْحِدِ</مقطع>
  <مقطع هوية='٨٠٥٦٢' رقم="٩٦">كَمَا تَقُوْلُ <قول هوية='٣٦٣٧١' رقم="٢٣">هُوَ أَحْسَنُ الْفِتْيَاْنِ وَأَجْمَلُهُ وَأَكْرَمُ بَنِيْهِ وأَنْبَلُهُ</قول>
  </مقطع>
  <مقطع هوية='٩٢٨٤٣' رقم="٩٧">وَلَا بُدَّ مِنْ هٰذَا لِأَنَّهُ لَا يَخْلُو الْفِعْلُ مِنْ مُضْمَرٍ أَوْ مُظْهَرٍ مَرْفُوْعٍ مِنْ الْأَسْمَاْءِ</مقطع>
  <مقطع هوية='١٧٨٦٤' رقم="٩٨">كَأَنَّكَ قُلْتَ إِذَا مَثَّلْتَهُ <قول هوية='٠٢٣٤٣' رقم="٢٤">ضَرَبَنِي مَنْ ثَمَّ وَضَرَبْتُ قَوْمَكَ</قول>
  </مقطع>
  <مقطع هوية='٦٥٢٤٤' رقم="٩٩">وَتَرْكُ ذٰلِكَ أَجْوَدُ وَأَحْسَنُ لِلتِّبْيَاْنِ الَّذِي يَجِيْءُ بَعْدَهُ</مقطع>
  <مقطع هوية='٢٣٣٣٢' رقم="١٠٠">فَأُضْمِرَ <حرف>مَنْ</حرف> لِذٰلِكَ</مقطع>
  <!-- bakkah says the following is an interpolation so he omits it -->
  <مقطع هوية='١٧٦٠٥' رقم="١٠١">[قَالَ الْأَخْفَشُ] فَهٰذَا رَدِيْءٌ فِي الْقِيَاْسِ</مقطع>
  <مقطع هوية='٥٤٨٧١' رقم="١٠٢">يَدْخُلُ عَلَيْهِ أَنْ تَقُوْلَ <قول هوية='٤٦٧٧٧' رقم="٢٥">أَصْحَاْبُكَ جَلَسَ</قول>
  </مقطع>
  <مقطع هوية='٨٣٠٢١' رقم="١٠٣">تُضْمِرُ شَيْئًا يَكُوْنُ في اللَّفْظِ وَاْحِدً</مقطع>
  <مقطع هوية='٦٩٢٨٧' رقم="١٠٤">فَقَوْلُهُمْ <قول هوية='٤٩٠٣١' رقم="٢٦">هُوَ أَظْرَفُ الْفِتْيَاْنِ وَأَجْمَلُهُ</قول>
  </مقطع>
  <مقطع هوية='٥٦٤٦٦' رقم="١٠٥">لَا يُقَاْسُ عَلَيْهِ</مقطع>
  <مقطع هوية='٣٩١٠٢' رقم="١٠٦">أَلَا تَرَى أَنَّكَ لَوْ قُلْتَ وَأَنْتَ تُرِيْدُ الْجَمَاْعَةَ <قول هوية='٢٣٠١١' رقم="٢٧">هٰذَا غُلَاْمُ الْقَوْمِ وَصَاحِبُهُ</قول> لَمْ يَحْسُنْ</مقطع>
